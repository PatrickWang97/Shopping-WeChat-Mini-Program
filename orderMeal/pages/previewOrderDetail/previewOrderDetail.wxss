page {
  background-color: #f4f4f4;
}
.page-top-nav {
  position: static;
  margin: 0;
}
.previewOrder-section {
  padding: 34rpx 36rpx;
  margin-bottom: 14rpx;
  background:#fdfdfd;
}
.previewOrder-goods-list {
  padding-top: 24rpx;
  padding-bottom: 24rpx;
}
.previewOrder-goods-item {
  position: relative;
}
.previewOrder-goto-homepage .icon-rightarrow {
  position:absolute;
  right:10px;
  display:block;
  font-size:18px;
}
.previewOrder-wrap .right-arrow-bar {
  line-height: 18px;
}
.previewOrder-wrap .right-arrow-bar:before {
  top: 7px;
}
.previewOrder-wrap .list-goods-cover {
  left: 0;
  width:117.2rpx;
  height:117.2rpx;
}
.previewOrder-wrap .list-goods-content {
  padding: 0 78px 0 80px;
  font-size: 14px;
}
.previewOrder-wrap .list-goods-title {
  color: #787878;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.previewOrder-wrap .list-goods-model {
  color: #aaa;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.previewOrder-wrap .list-goods-price {
  margin-top: 5px;
  color:#ff7223;
}
.payPage-wrap {
  background-color: #eee;
}
textarea {
  width: auto;
  height: 60px;
  padding: 10px;
  border: 1px solid #eee;
  background-color: #e9e7e8;
}
input {
  padding:10px;
  border:1px solid #eee;
  background-color: #e9e7e8;
}
.previewOrder-section-title {
  margin-bottom: 8px;
}
.previewOrder-section-title .icon-rightarrow {
  float: right;
  font-size: 16px;
}
.options-group {
  position: relative;
  display: block;
  width: 57%;
  margin: 0 auto;
  color: #919191;
  overflow: hidden;
}
.options-item-left,
.options-item-right {
  position:relative;
  z-index: 2;
  text-align: center;
}
.options-item-left {
  float: left;
}
.options-item-right {
  float: right;
}
.options-item-circle {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 5px;
  border-radius:50%;
  background-color: #a4a0a1;
  display: inline-block;
}
.options-item-circle.selected {
  background-color: #ff7100;
}
.options-group-crossband {
  position: absolute;
  left: 50rpx;
  right: 50rpx;
  top: 21rpx;
  height: 14rpx;
  background-color: #d2d2d2;
  z-index: 1;
}
.previewOrder-pay-btn {
  display: block;
  width: 180px;
  line-height: 38px;
  font-size: 14px;
  margin: 20px auto;
  border-radius: 5px;
  color: #fff;
  background-color: #ff7100;
}
.buy-goods-count{
  display:inline-block;
  width: 25px;
  height: 25px;
  line-height: 25px;
  margin: 0;
  box-sizing:content-box;
  text-align:center;
  vertical-align:middle;
  border: none;
}
.quantity .minus, .quantity .plus {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: normal;
  background-color: #ff7100;
  border-color: #ff7100;
  border-radius: 50%;
}
.quantity .minus {
  background-color: #fff;
  border-color: #333;
}
.quantity .plus::before, .quantity .minus::before {
  width: 30rpx;
  height: 5rpx;
}
.quantity .plus::after {
  width: 5rpx;
  height: 30rpx;
}
.quantity .plus::after, .quantity .plus::before {
  background-color: #fff;
}
.quantity .minus::before, .quantity .minus.disabled::before {
  background-color: #333;
}
.previewOrder-time-picker {
  padding: 10px;
  background-color: #e9e7e8;
  color: #333;
}
.previewOrder-discount-dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
  background-color: rgba(0,0,0,0.7);
}
.previewOrder-discount-dialog .previewOrder-section {
  margin-bottom: 0;
}
.discount-dialog-content {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}
.select-discount-title {
  line-height: 24px;
  color: #818181;
  word-wrap:break-word;
  display: flex;
  justify-content: space-between;
}
.select-discount-title .pull-left{
  width: 500rpx;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.discount-dialog-options {
  background-color: #f7f5f8;
  overflow: hidden;
}
.discount-dialog-options text {
  display: inline-block;
  padding: 34rpx 36rpx;
  min-width: 50px;
  text-align: center;
}
.previewOrder-discount-dialog .radio-group {
  padding-left: 20px;
}
.previewOrder-discount-dialog .radio-group label {
  display: block;
  margin: 10px 0;
  word-break: break-all;
  white-space: nowrap;
  overflow: hidden;
}
.location-section {
  display:inline-block;
  border:1px solid #ccc;
  margin-left:10px;
  line-height:24px;
  width: 50px;
  text-align: center;
  vertical-align: middle;
  border-radius:4px;
}
.location-section>view {
  overflow: hidden;
}
.previewOrder-section-location {
  margin-top: 10px;
  text-align: center;
}
.previewOrder-wrap .quantity .minus:before {
  background-color: #dedede;
}
.previewOrder-wrap .quantity .minus {
  border-color: #dedede;
}
.use-balance-option {
  line-height: 32px;
  overflow: hidden;
}
switch {
  margin-right: -4px;
}
.tostorePay-coupon-wrap .gift-text {
  display: inline-block;
  margin-left: 10px;
  border: 1px solid #ff7100;
  color:#ff7100;
  line-height:30rpx;
}
.previewOrder-goods-item .gift-text {
  display: inline-block;
  border: 1px solid #ff7100;
  color:#ff7100;
  line-height:30rpx;
}
.discount-way {
  position: relative;
}
.discount-way .previewOrder-section-title {
  position: absolute;
}
.discount-way .icon-rightarrow {
  position: absolute;
  right:60rpx;
  font-size:28rpx;
  color:#aaa;
  top:40rpx;
}
.discount-way .select-discount-title {
  padding-top:64rpx;
  position:relative;
  z-index:2;
}
.page-addshoppingcart-dialog .dialog-block-item {
  padding: 10px;
}
.page-addshoppingcart-dialog .thumb {
  float: left;
  width: 200rpx;
  height: 130rpx;
  margin: 4.68rpx 23.4rpx 0 0;
  position: relative;
}
.page-addshoppingcart-dialog .thumb .thumb-wrap {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  border: 1px solid #dedede;
  border-radius: 4rpx;
  background-color: #fff;
  overflow: hidden;
}
.page-addshoppingcart-dialog .thumb image {
  width: 100%;
  height: 100%;
}
.page-addshoppingcart-dialog .pay-goods-title {
  font-size: 32.76rpx;
  padding-right: 60rpx;
  line-height: 46.8rpx;
}
.page-addshoppingcart-dialog .pay-checked-text{
  font-size: 32.76rpx;
  line-height: 46.8rpx;
}
.page-addshoppingcart-dialog .page-dialog-close{
  display: block;
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #d2d2d2;
  color: #d2d2d2;
  text-align: center;
  line-height: 36rpx;
  border-radius: 50%;
  font-size: 37.44rpx;
  top: 23.4rpx;
  right: 23.4rpx;
}
.page-addshoppingcart-dialog .pay-goods-models {
  margin: -10px 0;
  max-height: 300px;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
.page-addshoppingcart-dialog .pay-goods-models .pills-list {
  display: inline-block;
  vertical-align: middle;
}
.page-addshoppingcart-dialog .pay-goods-models > view {
  margin: 0;
  padding: 10px 0;
  border-bottom: 1px solid #eee;
}
.page-addshoppingcart-dialog .pay-goods-models > view:last-child{
  border-bottom: none;
}
.page-addshoppingcart-dialog .pills-list label {
  color: #353535;
  margin-bottom: 2px;
  margin-top: 2px;
  border: none;
  background-color: #f5f5f5;
  border-radius: 8px;
}
.page-addshoppingcart-dialog .pills-list label.select {
  position: relative;
  color: #fff;
  background-color: #ff7100;
}
.page-addshoppingcart-dialog .pills-list label.select:after {
  content: '';
  position: absolute;
  bottom: 0;
  right: 0;
  width: 12px;
  height: 12px;
}
.toStore-bottom-nav {
  display: block;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  line-height: 110rpx;
  background-color: #fff;
  -webkit-user-select: none;
  user-select: none;
}
.bottom-nav-detail {
  display: inline-block;
  box-sizing: border-box;
  width: calc(100% - 200rpx);
  padding: 0 35rpx;
  background-color: #3f3f3f;
  color: #fff;
}
.bottom-nav-ready {
  display: inline-block;
  width: 200rpx;
  background-color: #ff7100;
  color: #fff;
  text-align: center;
}
.bottom-nav-ready.disabled {
  background-color: #a8a8a8;
  color: #fff;
}
.tostorePay-coupon-wrap .dialog-block-item {
  margin-bottom: 10px;
  font-size: 12px;
  border: none;
}
.tostorePay-coupon-wrap .pay-goods-title {
  text-align: center;
  padding-right: 30px;
  font-size: 16px;
}
.tostorePay-coupon-wrap .page-bottom-dialog {
  position: absolute;
  bottom: 0;
  width: 100%;
  padding-bottom: 45px;
  background: #fff;
}
.tostorePay-coupon-wrap .pills-list label {
  min-width: 42px;
  float: left;
  position: relative;
  display: block;
  padding: 3px 12px;
  border: 1px solid #dedede;
  border-radius: 15px;
  margin-left:10px;
  margin-right:10px;
  color: #707070;
}
.tostorePay-coupon-wrap .pay-goods-models view {
  margin: 0;
  padding: 2px 10px;
  border-bottom: none;
} 
.tostorePay-coupon-wrap .pills-list label.select {
  position: relative;
  color: #fff;
  background-color: #ff7100;
  border-color: #ff7100;
}
