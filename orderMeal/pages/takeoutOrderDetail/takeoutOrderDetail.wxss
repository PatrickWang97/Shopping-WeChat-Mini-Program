page {
  background-color: #f8f8f8;
  font-size: 31rpx;
}
.page-top-nav {
  position: static;
  margin: 0;
}
.order-detail-section {
  background: #fff;
  font-size:28rpx;
  padding-bottom: 30rpx;
  overflow: hidden;
}
.orderDetail-goto-homepage .icon-rightarrow {
  position: absolute;
  display: block;
  right: 20rpx;
  font-size: 36rpx;
}
.takeoutInfo {
  background:#fff;
  text-align: center;
  margin: 20rpx 20rpx 0 20rpx;
  border-radius: 8rpx;
  padding: 38rpx 0 36rpx 0;
}
.takeoutImage {
  width: 128rpx;
  height: 128rpx;
  display: inline-block;
  border-radius: 50%;
}
.takeoutInfo view{
  font-size: 34rpx;
  color: #333;
  font-weight: 500;
}
.takeoutInfo .icon-rightarrow {
  color: #D2D2D2;
}
.top-section {
  font-size: 36rpx;
  color: #fff;
  background-color: #535961;
}
.address-info-section {
  padding-top:26rpx;
  margin-top:-26rpx;
}
.orderDetail-pay-count {
  color: #d20e10;
}
.bottom-nav {
  padding: 20rpx 0;
  border-top: 1px solid #f2f2f2;
  color: #333;
  background-color: #fff;
}
.status-btn {
  padding-top: 32rpx;
}
.bottom-nav text,.status-btn text {
  display: inline-block;
  margin-right: 20rpx;
}
.goods-cover, .goods-content, .goods-right {
  display: inline-block;
  vertical-align: top;
}
.goods-list-item {
  margin: 0;
  padding: 30rpx 0;
}
.goods-list-item:first-child {
  margin-top: -10rpx;
}
.goods-cover {
  width: 132rpx;
  height: 132rpx;
}
.goods-content {
  width: calc(100% - 180rpx);
  vertical-align:middle;
}
.icon-closes {
  color: #333;
  margin:0;
  box-sizing:content-box;
  font-size:26rpx;
  text-align:left;
  vertical-align:middle;
  border:none;
  line-height:26rpx;
}
.icon-closes:before {
  font-size: 16rpx;
}
.goods-seckill-content{
  width: 338rpx;
}
.goods-model {
  margin-top: 10rpx;
  color: #aaa;
}
.goods-right {
  width: 140rpx;
  text-align: right;
}
.goods-right view {
  display: inline-block;
}
.goods-seckill-right{
  width: 200rpx;
}
.freightAdress-tip {
  margin-top: 10px;
  margin-left: 6px;
  font-size: 12px;
  color: #999;
}
.freightAdress-ico {
  font-size:32rpx;
  margin:0 10rpx;
}
.freightAdress-phone {
  margin-top: 20rpx;
}
.location-name {
  text-decoration: underline;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: inline-block;
  width: 90%;
  overflow: hidden;
}
.btn {
  padding: 18rpx 30rpx;
  font-size: 26rpx;
  line-height: 1;
  border-radius: 2px;
}
.section-title, .section-content {
  display: inline-block;
  vertical-align: middle;
}
.section-title {
  width: 5em;
}
.section-content {
  margin-left: 2em;
  width: calc(100% - 7em);
  text-align: right;
}
.goods-seckill-price{
  font-size: 38rpx;
}
.goods-original-price{
  font-size: 29rpx;
  text-decoration: line-through;
  margin-left: 8rpx;
}
.goods-seckill-sign{
  font-size: 13px;
  color: #ff7100;
  margin-top: 5px;
}
.transport-name {
  vertical-align: middle;
  font-size: 30rpx;
  color: #333;
  margin-left: 24rpx;
}
.status-btn .refund {
  font-size:24rpx;
  color:#666666;
  text-align:left;
  padding: 0 28rpx 0 30rpx;
}
.status-btn .refund text {
  margin-left: 12rpx;
  position: relative;
}
.status-btn .refund text:after {
  content: "";
  display: inline-block;
  height: 6rpx;
  width: 6rpx;
  border-radius: 50%;
  background-color: #D2D2D2;
  position: absolute;
  left: -12rpx;
  top: 18rpx;
  vertical-align: middle;
}
.shopImage {
  width:44rpx;
  height:44rpx;
  display:inline-block;
  border-radius:50%;
  margin-right: 16rpx;
  color: #303445;
  vertical-align: middle;
}
.info-takeout-phone {
  float: right;
}
.inner-content .waimai-img {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
}
.inner-content .waimai-title {
  display: inline-block;
}
.waimai-title {
  font-size: 22rpx;
}
.waimai-title-text {
  font-size: 26rpx;
  font-weight: 400;
  max-width: 400rpx;
  overflow: hidden;
  vertical-align: middle;
  text-overflow: ellipsis;
  white-space: nowarp;
  word-break: keep-all;
}
.inner-content .totle-price {
  width:55px;
  vertical-align:middle;
  font-size:26rpx;
  text-align:right;
  float:right;
  height:90rpx;
  line-height:90rpx;
}
.order-detail-section .classname{
  border-radius:4rpx;
  margin-right:20rpx;
  font-size: 24rpx;
  width:100rpx;
  height:32rpx;
  display:inline-block;
  line-height:32rpx;
  text-align:center;
}
.orange {
  color: #FF7100;
  border:1px solid rgba(255,113,0,0.2);
  background-color:#FFF1E5;
}
.pink {
  color: #F2303E;
  border:1px solid rgba(242,48,62,0.2);
  background-color:#FEEFF0;
}
.order-detail-section .deliver_title {
  padding:30rpx 0rpx 22rpx;
  background:#fff;
  font-weight: 500;
  border-bottom:1px solid #F5F6F7;
  margin-bottom: 26rpx;
}
.order-detail-section .line {
  border: 1px solid #F5F6F7;
  margin: 0 16rpx;
  height:26rpx;
  display:inline-block;
  vertical-align:middle;
}
.order-detail-section .order-remark {
  word-break:break-all;
  display: block;
  white-space: normal;
  overflow: auto;
  width: 440rpx;
  height: auto;
  text-align: right;
  color:#666;
  float: right;
}
.status-btn .selectMore {
  display: inline-block;
  width: 140rpx;
  vertical-align: top;
  border: 1px solid #D2D2D2;
  background-color: #fff;
  border-radius: 4rpx;
  color: #666666;
}
.status-btn .selectMore view{
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.selectMore view{
  color: #666666;
}
.instruction-warp {
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0,0,0,0.5);
  top: 0;
  z-index: 99;
}
.instruction-container {
  background-color: #fff;
  position: absolute;
  width: 560rpx;
  margin-left: 48rpx;
  top: 186rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
}
.instruction-container text{
  display: inline-block;
  width: 100%;
}
.instruction-container .instruction-item {
  line-height: 44rpx;
}
.instruction-container .instruction-title {
  text-align: center;
  font-size: 30rpx;
  padding: 46rpx 0 22rpx 0;
  letter-spacing:2rpx;
  display: flex;
  justify-content: center;
}
.instruction-container .instruction-title icon {
  margin-right: 12rpx;
}
.instruction-container .instruction-title text {
  width: auto;
}
.instruction-container .instruction-close {
  text-align: center;
  color: #FF7100;
  height: 90rpx;
  line-height: 90rpx;
  border-top: 1px solid #EBEBEB;
  font-size: 30rpx;
  margin-top: 40rpx;
}