.coupon-detail-wrap {
  display: none;
  margin: 28rpx 24rpx 0;
  border-radius: 6rpx;
  background: #fff;
}
.coupon-detail-wrap.show {
  display: block;
}
.coupon-detail-wrap .top-section {
  position: relative;
  width: 100%;
  height: 100rpx;
  background: #fff;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  color: #666666;
}
.coupon-detail-wrap .top-section::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 9rpx;
  width: calc(100% - 20rpx);
}
.coupon-detail-wrap .top-section .logo {
  position: absolute;
  top: 21rpx;
  left: 40rpx;
  width: 59rpx;
  height: 59rpx;
  border-radius: 50%;
}
.coupon-detail-wrap .top-section .name {
  position: absolute;
  top: 35rpx;
  left: 120rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 28rpx;
}
.coupon-detail-wrap .top-section .icon-rightarrow{
  font-size: 16rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-weight: bold;
  color: #666666;
  position: relative;
  left: 20rpx;
  top: -2rpx;
}
.coupon-detail-wrap .top-section .share {
  position: absolute;
  top: 35rpx;
  right: 30rpx;
  width: 32rpx;
  height: 32rpx;
  color: #8a8b8c;
  font-size: 32rpx;
  line-height: 32rpx;
}
.coupon-detail-wrap .top-section .share .share-btn {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.coupon-detail-wrap .division-line-one {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}
.coupon-detail-wrap .division-line-one .left-circle{
  position:absolute;
  top:-12rpx;
  left:-14rpx;
  width:20rpx;
  height:20rpx;
  background:#eee;
  transform:rotate(45deg);
}
.coupon-detail-wrap .division-line-one .right-circle{
  position:absolute;
  top:-12rpx;
  right:-14rpx;
  width:20rpx;
  height:20rpx;
  background:#eee;
  transform:rotate(45deg);
}
.coupon-detail-wrap .middle-bottom-block{
