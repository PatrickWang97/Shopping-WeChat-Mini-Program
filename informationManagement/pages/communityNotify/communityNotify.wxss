.community-li-cover{
  display: inline-block;
  width: 40px;
  height: 40px;
  border-radius: 20px;
  border: 1px solid #eee;
  overflow: hidden;
  vertical-align: bottom;
}
.community-li-cover > image{
  width: 100%;
  height: 100%;
}
.community-li-author{
  display: inline-block;
  margin-left: 10px;
  vertical-align: bottom;
}
.community-li-name{
  display: block;
  margin-bottom: 5px;
}
.community-li-time{
  font-size: 12px;
  color: #959595;
}
.communityNotify-wrap{
  background-color: #eee;
  height: 100vh;
  overflow: hidden;
  font-size: 14px;
  word-break: break-all;
}
.communityNotify-head{
  height: 45px;
  color: #3a3a3a;
  background-color: #f8f8f8;
}
.communityNotify-back{
  float: left;
  line-height: 45px;
  margin-left: 10px;
  width: 30px;
  text-align: center;
  font-size: 16px;
}
.communityNotify-tab{
  text-align: center;
}
.communityNotify-tab > view{
  display: inline-block;
  width: 60px;
  line-height: 44px;
  margin: 0 10px;
}
.communityNotify-tab > view.active{
  color: #00b6f8;
  border-bottom: 1px solid #00b6f8;
}
.communityNotify-tab > view > text.have-messages {
  position: relative;
}
.communityNotify-tab > view > text.have-messages::after {
  content: '';
  position: absolute;
  top: -2rpx;
  right: -8rpx;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color: #FA1E1E;
}
.communityNotify-content{
  position: absolute;
  top: 45px;
  bottom: 0;
  left: 0;
  right: 0;
  overflow: auto;
}
.communityNotify-scrollwrap{
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
  display: none;
}
.communityNotify-scrollwrap.active{
  display: block;
}
.communityNotify-li{
  margin-top: 10px;
  background-color: #fff;
  padding: 15px;
}
.communityNotify-li-content{
  margin: 10px 0;
}
.communityNotify-li-post{
  display: flex;
  padding: 8px;
  background-color: #f4f4f4;
}
.communityNotify-li-post > image{
  width: 40px;
  height: 40px;
  vertical-align: top;
  margin-right: 15px;
}
.communityNotify-li-post-text{
  flex: 1;
  display: inline-block;
  font-size: 12px;
  line-height: 20px;
  color: #959595;
  vertical-align: top;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-inline-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.communityNotify-li-none{
  padding: 15px 0;
  text-align: center;
  font-size: 12px;
  color: #949494;
}
.communityNotify-replyto{
  color: #577a9f;
  margin-right: 5px;
}
.communityNotify-li-top {
  line-height: 20px;
  color: #333;
  font-size: 14px;
  margin-bottom: 10px;
  word-break:break-all;
}
.article-info {
  position: relative;
  padding: 10px;
  background: #eee;
}
.article-info .article-img {
  position: absolute;
  top: 10px;
  left: 10px;
  margin-right: 10px;
  width: 75px;
  height: 75px;
  background: #d1d1d1;
}
.article-info .article-img image {
  width: 100%;
  height: 100%;
}
.article-info .article-text {
  padding-left: 85px;
  height: 75px;
}
.article-info .article-title {
  margin-bottom: 10px;
  height: 26px;
  line-height: 26px;
  color: #666;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.article-info .article-description {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 20px;
  font-size: 12px;
  color: #666;
}
.tcommunity-topic-link {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
  padding: 14rpx;
  box-sizing: border-box;
  height: 100rpx;
  background-color: #F5F5F5;
}
.tcommunity-topic-video {
  position: relative;
  height: 360rpx;
  overflow: hidden;
}
.tcommunity-topic-link .link-logo-wrap {
  position: relative;
  margin-right: 15rpx;
}
.tcommunity-topic-link .link-logo {
  display: block;
  width: 72rpx;
  height: 72rpx;
}
.tcommunity-topic-link .icon-news-video-play,
.tcommunity-topic-video .icon-news-video-play {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: rgba(0,0,0,.5);
  transform: translateX(-50%) translateY(-50%);
  border-radius: 50%;
}
.tcommunity-topic-video .icon-news-video-play {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 25rpx;
}
.tcommunity-topic-video .video-cover {
  display: block;
  width: 100%;
  height: 100%;
}
.tcommunity-topic-link .link-text {
  flex: 1;
  overflow: hidden;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 26rpx;
  color: #666;
}
