  line-height:16px;
  color:#fff;
  border-radius:1px;
}
.community-stick::before {
  display: block;
  content: '置顶';
  transform: scale(.8);
}
.community-hot::before {
  display: block;
  content: '精品';
  transform: scale(.8);
}
.community-hot {
  background: #FE644A;
}
.community-stick {
