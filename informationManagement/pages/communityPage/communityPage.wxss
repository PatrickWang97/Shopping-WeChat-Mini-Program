  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 70rpx;
  color: #010101;
  font-size: 30rpx;
}
.community-li-time{
  display: block;
  font-size: 24rpx;
  color: #999;
}
.community-hot,
.community-stick {
  display:inline-block;
