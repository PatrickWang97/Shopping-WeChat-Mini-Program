.live-wrap {
  width: 654rpx;
  margin: 0 auto;
  padding: 24rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 18rpx 0px rgba(0, 0, 0, 0.1);
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.live-wrap .live-room {
  position: relative;
  width: 100%;
  height: 360rpx;
  border-radius: 6rpx;
  overflow: hidden;
}
.live-wrap .live-room image {
  display: block;
  width: 100%;
  height: 100%;
}
.live-room .live-online {
  position: absolute;
  top: 14rpx;
  left: 16rpx;
  color: #fff;
  padding: 7rpx 7rpx 7rpx 27rpx;
  height: 32rpx;
  background: linear-gradient(90deg, rgba(255, 84, 57, 1) 0%, rgba(255, 104, 151, 1) 100%);
  border-radius: 4rpx;
  font-size: 20rpx;
}
.live-room .live-online::before,
.live-room .live-notice .live-notice-title::before {
  content: "";
  position: absolute;
  left: 9rpx;
  top: 16rpx;
  width: 12rpx;
  height: 12rpx;
  background-color: #fff;
  border-radius: 50%;
}
.live-room .live-notice {
  position: absolute;
  top: 14rpx;
  left: 16rpx;
  display: flex;
  align-items: center;
  font-size: 20rpx;
  color: #fff;
}
.live-room .live-subscribe {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: linear-gradient(270deg, rgba(0, 217, 169, 1) 0%, rgba(0, 205, 126, 1) 100%);
  border-radius: 6rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  transform: translate(-50%, -50%);
}
.live-room .live-notice .live-notice-title {
  height: 32rpx;
  padding: 7rpx 8rpx 7rpx 25rpx;
  border-radius: 4rpx;
  z-index: 2;
}
.live-room .live-notice .live-notice-title.green-color {
  background: linear-gradient(270deg, rgba(0, 217, 169, 1) 0%, rgba(0, 205, 126, 1) 100%);
}
.live-room .live-notice .live-notice-title.blue-color {
  background: linear-gradient(270deg, rgba(53, 160, 255, 1) 0%, rgba(39, 112, 254, 1) 100%);
}
.live-room .live-notice .live-notice-time {
  padding: 7rpx 10rpx;
  height: 32rpx;
  background: rgba(0, 0, 0, 1);
  border-radius: 4rpx;
  opacity: 0.3;
  margin-left: -2rpx;
  z-index: 1;
}
.live-room .live-anchor {
  display: flex;
  align-items: center;
  height: 50rpx;
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
}
.live-room .live-anchor .live-anchor-head {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  overflow: hidden;
  background-color: rgb(255, 246, 239, 0.6);
}
.live-room .live-anchor .live-anchor-name {
  margin-left: 10rpx;
  color: #fff;
  font-size: 22rpx;
}
.live-wrap .live-title {
  line-height: 32rpx;
  margin-top: 24rpx;
