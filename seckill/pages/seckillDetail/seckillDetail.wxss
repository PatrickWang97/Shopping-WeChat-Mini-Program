.leaderInfo-dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 100;
}
.leaderInfo-dialog .avatar {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 20rpx;
}
.community-list-content {
  margin-top: 30rpx;
  padding-bottom: 34rpx;
  background-color: #fff;
  border-radius: 12rpx;
  border-bottom: 1px solid #F3F3F3;
}
.list-address-item {
  display: -webkit-flex;
  display: flex;
  min-height: 40rpx;
  padding-top: 18rpx;
}
.list-address-item>view:nth-child(1) {
  flex: 0 0 316rpx;
  color: #666;
  align-self: flex-start;
}
.list-address-item>view:nth-child(1) .ico-moon {
  margin-right: 10rpx;
  color:#999999;
  font-size:28rpx;
  vertical-align: middle;
  margin-left:2rpx;
}
.list-address-item>view:nth-child(1) label {
  width: 130rpx;
  word-break: break-all;
  vertical-align: middle;
}
.list-address-item .ico-moon {
  line-height: 38rpx;
  flex: 0 0 24rpx;
  margin-right: 12rpx;
}
.list-address-item label {
  color: #999999;
}
.leaderInfo-dialog-view {
  width: 540rpx;
  max-height: 800rpx;
  overflow-y: auto;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateY(-50%) translateX(-50%);
  border-radius: 10rpx;
  font-size: 26rpx;
  padding: 46rpx 40rpx 40rpx 50rpx;
}
.flex330 {
  flex: 0 0 330rpx;
}
.select-leader {
