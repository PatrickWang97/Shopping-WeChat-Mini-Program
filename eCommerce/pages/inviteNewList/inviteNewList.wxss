.page-wrap {
  background: #f3f3f3;
  min-height: 100vh;
}
.page-mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .4);
  position: fixed;
}
.page-head {
  padding: 30rpx 30rpx 1rpx;
  background: #fff;
}
.input-wrap {
  display: flex;
  background: #f5f5f5;
  border-radius: 32rpx;
  align-items: center;
}
.page-input {
  height: 64rpx;
  background: #f5f5f5;
  border-radius: 32rpx;
  flex: 1;
}
.input-search-icon {
  font-size: 28rpx;
  color: #b3b3b3;
  padding: 0 20rpx 0 30rpx;
}
.input-placeholder-style {
  font-size: 28rpx;
  color: #ccc;
}
.bread-wrap {
  background: #fff;
  padding: 32rpx 80rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.bread-type {
  font-size: 30rpx;
  color: #333;
  position: relative;
}
.bread-type.active {
  font-weight: bold;
}
.bread-border {
  position: absolute;
  bottom: -4rpx;
  left: 0;
  width: 100%;
  height: 10rpx;
  background:linear-gradient(90deg,rgba(255,113,0,1),rgba(255,255,255,1));
}
.flex-between {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.userList {
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  margin-bottom: 18rpx;
  min-height: 88rpx;
}
.userList.type1 {
  padding: 22rpx 20rpx;
  background: #fff;
  box-sizing: border-box;
}
.userList.height {
  height: 930rpx;
}
.userList-item {
  padding: 50rpx 30rpx;
  background: #FAFAFA;
  border-radius: 12rpx;
  position: relative;
  margin-bottom: 22rpx;
}
.userList-name {
  font-size: 26rpx;
  font-weight: bold;
}
.userList-timestamp {
  font-size: 24rpx;
  color: #999;
}
.record-badge {
  color: #fff;
  font-size: 22rpx;
  background:linear-gradient(-3deg,rgba(236,72,2,1),rgba(255,126,10,1));
  border-radius:15rpx 15rpx 15rpx 4rpx;
  padding: 6rpx 14rpx;
  position: absolute;
  right: -6rpx;
  top: -6rpx;
}
.userList-tips {
  font-size: 24rpx;
  color: #666;
  padding-bottom: 40rpx;
  margin-left: 20rpx;
}
.user-list-none {
  text-align: center;
  padding-top: 130rpx;
}
.user-list-none image {
  width: 266rpx;
  height: 196rpx;
}
.user-list-none-tip {
  margin-top: 40rpx;
  color: #666666;
  font-size: 26rpx;
}
.invite_wrap {
  margin: 20rpx 24rpx;
}
.invite_wrap_head {
  background: #EDEDED;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.bread_level_type {
  line-height: 88rpx;
  text-align: center;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  width: 100%;
}
.bread_level_type.active {
  background: #fff;
  font-weight: bold;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
}
