.traceGoodsList-page{
  padding-top: 20rpx;
}
.trace-company-list{
  background: #fff;
  margin-bottom: 20rpx;
}
.trace-company-item{
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: solid 1rpx #F3F3F3;
  padding: 0 26rpx;
  font-size: 26rpx;
  color: #333333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.trace-goods-item{
  display: flex;
  padding: 31rpx 30rpx;
}
.check-goods{
  width: 91rpx;
  text-align: center;
  flex-shrink:0;
  color: #3091F2;
  font-size:34rpx;
}
.trace-goods-image{
  width: 100rpx;
  height: 100rpx;
  flex-shrink: 0
}
.trace-goods-content{
  padding-left: 20rpx;
  width: 100%;
  position: relative;
}
.trace-goods-content-title{
  width:314rpx;
  font-size:26rpx;
  color:rgba(51,51,51,1);
  line-height:34rpx;
  word-break: break-all;
  position: relative;
  top: -4rpx;
}
.trace-goods-content-model{
  width:314rpx;
  font-size:24rpx;
  color:rgba(153,153,153,1);
  line-height:32rpx;
  margin-top: 11rpx;
}
.trace-goods-num{
  font-size: 24rpx;
  position: absolute;
  right: 0;
  top: 0;
  color: #999999;
}
.trace-goods-line{
  width: 690rpx;
  margin: 0 auto;
  height: 2rpx;
  background: #F3F3F3;
}
.model-value-content{
  margin-right: 15rpx;
}