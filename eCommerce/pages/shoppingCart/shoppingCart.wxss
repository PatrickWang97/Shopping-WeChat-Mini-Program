}
.goods-delivery-method > text.selected {
  font-size: 28rpx;
  color: #333;
  font-weight:bold;
}
.goods-delivery-method > text.selected .bottom-line {
  display: inline-block;
  width: 50rpx;
  height: 4rpx;
