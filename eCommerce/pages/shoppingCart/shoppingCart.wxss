  padding: 23rpx 24rpx 0;
}
.cart-header {
  background: #fff;
  padding: 30rpx 0 30rpx 30rpx;
  display: flex;
}
.cart-header view { 
  display: inline-block;
  vertical-align: middle;
}
view.cart-type-box {
  border-radius: 30rpx;
  overflow: hidden;
  font-size: 26rpx;
  color: #999;
  width: 510rpx;
  display: flex;
  flex: 3;
  background: #F0F0F0;
}
.cart-type-box text {
  display: inline-block;
  line-height: 50rpx;
  text-align: center;
  width: 100%;
  border-radius: 30rpx;
}
.cart-type-box text.selected {
  background: #ff7100;
  color: #fff;
}
.cart-type-box text:last-child {
  border: 0;
}
.cart-type-title {
  line-height: 84rpx;
  font-size: 26rpx;
  color: #333;
  padding: 0 30rpx;
}
.goods-delivery-method {
  display: flex;
