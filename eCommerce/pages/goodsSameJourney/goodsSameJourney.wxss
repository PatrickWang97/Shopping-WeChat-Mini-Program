page {
  background: #f3f3f3;
}
map {
  width: 100%;
  height: 400rpx;
}
.delivery-list {
  background: #fff;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f3f3f3;
}
.delivery-list-box>.delivery-list:last-child {
  border-bottom: none;
}
.delivery-list .icon-goods-location {
  display: inline-block;
  vertical-align: middle;
  margin-right: 19rpx;
  color: #999;
}
.delivery-list-content {
  width: 528rpx;
  display: inline-block;
  vertical-align: middle;
}
.delivery-list-title {
  font-size: 28rpx;
  color: #333333;
  line-height: 28rpx;
}
.delivery-list-detail {
  font-size: 26rpx;
  color: #666;
  line-height: 36rpx;
  margin-top: 10rpx;
  margin-right: 30rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
}
.delivery-check-box {
  display: inline-block;
  color: #D9D9D9;
  vertical-align: middle;
  font-size: 34rpx;
}
.delivery-check-box.checked {
  color: #ff7100;
}
.sure-delivery {
  position: fixed;
  bottom: 0;
  color: #fff;
  background: #fff;
  width: 100%;
  line-height: 102rpx;
  font-size: 30rpx;
  text-align: center;
}
.delivery-list-box {
  overflow-y: auto;
  border-radius: 0 0 12rpx 12rpx;
}
.receipt-header {
  line-height: 94rpx;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1px solid #f3f3f3;
  font-weight:bold;
}
.operat-address {
  color: #ff7100;
  font-size: 34rpx;
}
.current-location-box {
  line-height: 88rpx;
  font-size: 0;
  background: #fff;
  padding: 0 30rpx;
}
.current-location-box text {
  display: inline-block;
  vertical-align: middle;
  color: #333;
  font-size: 28rpx;
}
.current-location-box .icon-goods-location {
  font-size: 30rpx;
  color: #999;
  margin-right: 20rpx;
}
.current-location-box .renew-position {
  color: #ff7100;
}
.address-label {
  font-size: 22rpx;
  width: 66rpx;
  text-align: center;
  margin-right: 10rpx;
  border-radius: 4rpx;
  vertical-align: middle;
  line-height: 32rpx;
  display: inline-block;
}
.home {
  background: #FFEFE2;
  color: #ff7100;
}
.company, .default {
  background: #EAF4FE;
  color: #3091f2;
}
.school {
  background: #F1FBEA;
  color: #79DC31;
}
.support-same-city {
  padding: 0 10rpx;
  background: #FFEFE2;
  color: #ff7100;
}
.no-support-pick .address-label {
  border: 1px solid rgba(153,153,153,0.5);
  background: #F7F7F7;
}
.no-support-header {
  margin-top: -90rpx;
  box-sizing: border-box;
  font-weight: normal;
}
.no-support-pick .delivery-list-title,
.no-support-pick .delivery-list-detail {
  color: #999;
}
.same-journey-null {
  color: #999;
  font-size: 26rpx;
  text-align: center;
  margin-top: 60rpx;
}
.same-journey-null image {
  width: 110rpx;
  height: 90rpx;
  margin-bottom: 30rpx;
}
.new-class {
  box-sizing: border-box;
  padding: 20rpx 24rpx 130rpx;
}
.top-delivery {
  box-sizing: border-box;
  background: #fff;
  border-radius: 12rpx;
  padding: 0 30rpx;
}
.sure-button{
  background: #FF7100;
  width: 710rpx;
  display: inline-block;
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 18rpx;
  border-radius: 44rpx;
}
.bottom-delivery {
  box-sizing: border-box;
  padding: 0 30rpx;
  background: #fff;
  border-radius: 12rpx 12rpx 0 0;
}