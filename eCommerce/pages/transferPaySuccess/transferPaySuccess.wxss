}
.settlement-activity>.card-title {
  position: relative;
  left: 50%;
  transform: translate(-50%);
  margin: 40rpx 0 30rpx 0;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
}
.settlement-activity>.card-title::before,
.settlement-activity>.card-title::after {
  content: '';
  position: absolute;
  width: 100rpx;
  height: 2rpx;
  background-color: #CCCCCC;
  top: 18rpx;
}
.settlement-activity>.card-title::before {
  left: 220rpx;
}
.settlement-activity>.card-title::after {
  right: 220rpx;
}
.settlement-activity .card-item {
  margin-top: 0;
  padding: 60rpx 30rpx 25rpx 20rpx;
}
.settlement-activity .card-detail {
  display: flex;
  position: relative;
  margin-bottom: 50rpx;
}
.settlement-activity .card-detail::before{
  content: '';
  position: absolute;
  top: 50px;
  left: -12px;
  width: 12px;
  height: 6px;
  border-radius: 6px 6px 0 0;
  border-width: 1px 1px 0 0;
  border-color: #FAFAFA;
  border-style: solid;
  transform: rotate(90deg);
  background: #FFF;
}
.settlement-activity .card-detail::after{
  content: '';
  position: absolute;
  top: 50px;
  right: -18px;
  width: 12px;
  height: 6px;
  border-radius: 6px 6px 0 0;
  border-width: 1px 1px 0 0;
  border-color: #FAFAFA;
  border-style: solid;
  transform: rotate(270deg);
  background: #FFF;
}
.settlement-activity .card-detail .cover-thumb{
  flex: 0 0 80rpx;
  height: 80rpx;
  margin-right: 28rpx;
  border-radius: 50%;
}
.settlement-activity .card-detail .title-warpper{
  flex: 0 0 auto;
}
.settlement-activity .card-detail .nickname{
  margin-bottom: 20rpx;
  line-height: 1;
  font-size: 30rpx;
  color: #333333;
}
.settlement-activity .card-detail .card-name{
  font-size: 26rpx;
  color: #999999;
}
.settlement-activity .card-detail .card-navigate{
  position: absolute;
  top: 10rpx;
  right: 0;
  width: 150rpx;
  line-height: 50rpx;
  text-align: center;
