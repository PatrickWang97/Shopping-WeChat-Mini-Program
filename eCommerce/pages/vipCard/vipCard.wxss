#vipCard {
  display: block;
}
#vipCard .card-wrap {
  padding-top: 30px;
  background-color: #f3f3f4;
}
#vipCard .card-wrap .card {
  position: relative;
  height: 355rpx;
  width: 660rpx;
  background: rgb(61, 183, 248);
  margin: 0 auto;
  border-radius: 10px;
  background-size: 100% 100%;
  color: #fff;
  font-size: 26rpx;
}
#vipCard .card .bg-img {
  width: 100%;
  height: 100%;
  margin-top: -4px;
}
#vipCard .card .logo {
  height: 90rpx;
  width: 90rpx;
  overflow: hidden;
  position: absolute;
  left: 40rpx;
  top: 40rpx;
}
#vipCard .card .title {
  position: absolute;
  top: 40rpx;
  left: 160rpx;
  width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
  font-weight: bold;
}
#vipCard .card .level {
  position: absolute;
  top: 48px;
  left: 80px;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 24rpx;
}
#vipCard .card .number {
  position: absolute;
  bottom: 45rpx;
  left: 40rpx;
}
#vipCard .card .duration {
  position: absolute;
  bottom: 45rpx;
  right: 40rpx;
}
#vipCard .card-content {
  position: relative;
  margin-top: -24rpx;
  background-color: #fff;
  font-size: 28rpx;
  z-index: 1;
}
#vipCard .card-content .item-title {
  position: relative;
  height: 110rpx;
  line-height: 110rpx;
  text-indent: 1em;
  padding-left: 24rpx;
  font-weight: bold;
}
#vipCard .item-title .ico-moon {
  position: absolute;
  top: 15px;
  right: 26px;
  line-height: 20px;
  text-indent: 0;
  color: #d2d2d2;
  transition: transform 1s;
}
#vipCard .item-title.active .ico-moon {
  transform: rotate(180deg);
}
#vipCard .card-content .item-body {
  display: none;
}
#vipCard .card-content .item-body.active {
  display: block;
}
.vipCard-rights .content > view {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 20rpx;
}
.vipCard-rights .content > view:last-of-type {
  margin-bottom: 0 !important;
}
.vipCard-rights .content > view .item {
  margin-bottom: 0 !important;
}
.vipCard-rights, .vipCard-notice, .vipCard-points, .vipCard-contact {
  width: 600rpx;
  padding: 25rpx;
  background-color: #fafafc;
  border-radius: 10rpx;
  margin: 0 auto;
}
.vipCard-notice .content, .vipCard-points .content,
.vipCard-contact .content {
  display: flex;
  flex-wrap: wrap;
}
.vipCard-rights .content .item, .vipCard-notice .content .item,
.vipCard-points .content .item, .vipCard-contact .content .item,
.vipCard-contact .content .item {
  line-height: 46rpx;
  margin-bottom: 20rpx;
}
.item-body .content .item:nth-child(odd) {
  flex: 0 0 30%;
  color: #333;
}
.item-body .content .item:nth-child(even) {
  flex: 0 0 70%;
  color: #999;
  word-break: break-all;
}
#vipCard .no-receive-card {
  height: 100vh;
  background: #fff;
}
.no-receive-card .tip-area {
  display: block;
  padding-top: 130px;
  text-align: center;
}
.no-receive-card .tip-img {
  display: block;
  width: 240rpx;
  height: 180rpx;
  margin: 0 auto 44rpx;
}
.no-receive-card .tip-text-1 {
  display: block;
  margin-bottom: 20rpx;
  color: #888;
  font-size: 40rpx;
}
.no-receive-card .tip-text-2 {
  display: block;
  color: #b2b2b2;
  font-size: 28rpx;
}
.receive-btn {
  width: 690rpx;
  height: 80rpx;
  margin: 50rpx auto;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  line-height: 80rpx;
  border-radius: 4rpx;
  background-color: #ff7100;
}
#vipCard .icon-qr-code {
  position: absolute;
  right: 40rpx;
  top: 40rpx;
  font-size: 70rpx;
}
#vipCard .vip-loading-strip {
  position: absolute;
  top: 140rpx;
  left: 40rpx;
  width: calc(100% - 80rpx);
  height: 4rpx;
  margin-top: 24rpx;
  background-color: rgba(255, 255, 255, 0.4);
}
#vipCard .vip-loading-strip .vip-loading-progress {
  position: absolute;
  left: 0;
  top: 0;
  max-width: 100%;
  width: 50%;
  height: 4rpx;
  background-color: #CAA661;
}
#vipCard .vip-loading-strip .vip-loading-condition {
  position: absolute;
  left: 0;
  top: 23rpx;
  max-width: calc(100% - 80rpx);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
}
#vipCard .content .item-dotted::before {
  content: "";
  display: inline-block;
  width: 8rpx;
  height: 8rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  background-color: #e5c588;
  vertical-align: middle;
}
#vipCard .content .bottom-item {
  margin-bottom: 0;
}
.shops-area {
  margin-bottom: 30rpx;
  border-bottom: 1px solid #dcdcdc;
}
.shopsList-wrapper {
  margin: 0 30rpx;
  color: #999;
  background-color: #fff;
}
.shops-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0 20rpx;
  color: #333;
}
.shops-title .icon-rightarrow {
  font-size: 26rpx;
  color: #B6B6B6;
}
.shops-list .shops-item {
  padding: 30rpx 0;
}
.shops-item .item-name-distance {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16rpx;
}
.shops-item .item-name {
  max-width: 520rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  color: #333;
}
.shops-item .item-distance {
  font-size: 22rpx;
}
.shops-item .item-address {
  font-size: 26rpx;
  color: #999;
}
.shopsList-more {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 30rpx 0;
  font-size: 28rpx;
  line-height: 28rpx;
  text-align: center;
  color: #666;
}
.shopsList-more .icon-downarrow {
  padding-left: 18rpx;
  font-size: 26rpx;
  color: #B6B6B6;
}
.thin-bd-b,
.thin-bd-t {
  position: relative;
}
.thin-bd-b:before,
.thin-bd-t:before{
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  width: 100%;
  background-color: #EEF0F7;
  transform: scaleY(0.5);
  box-sizing: border-box;
}
.thin-bd-b:before {
  bottom: 0;
}
.thin-bd-t:before {
  top: 0;
}