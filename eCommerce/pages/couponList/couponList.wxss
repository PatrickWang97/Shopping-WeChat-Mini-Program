page {
  height: 100%;
  background-color: #f6f6f6;
}
.tabs {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  background: #fff;
}
.tabs .tabs-wrap {
  height: 100%;
  width: 90%;
  margin: 0 auto;
  display: flex;
  font-size: 28rpx;
}
.tabs .tabs-wrap .tabs-wrap-item {
  flex: 1;
  height: 85rpx;
  color: #666;
}
.tabs .tabs-active-line{
  width:50rpx;
  height:4rpx;
  background:rgba(255,113,0,1);
  border-radius:2rpx;
  margin: 0 auto;
  position: relative;
  bottom: 16rpx;
}
.tabs .active {
  font-weight: bold;
  color: #333!important;
}
.ticket-list {
  height: calc(100vh - 108rpx);
  padding-top: 108rpx;
}
.zc-toast-bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: none;
  background: rgba(1,1,1,0.5);
  z-index: 999;
}
.zc-toast-bg.show {
  display: block;
}
.zc-toast {
  position: absolute;
  top: 350rpx;
  left: 50%;
  margin-left: -200rpx;
  width: 400rpx;
  height: 165rpx;
  background: #fff;
  border-radius: 6px;
}
.zc-toast .icon {
  position: absolute;
  top: 38rpx;
  left: 95rpx;
}
.zc-toast .title {
  position: absolute;
  top: 43rpx;
  left: 150rpx;
  height: 35rpx;
  line-height: 35rpx;
  color: #333;
  font-size: 35rpx;
}
.zc-toast .content {
  position: absolute;
  top: 95rpx;
  left: 130rpx;
  width: 100%;
  height: 26rpx;
  line-height: 26rpx;
  color: #666;
  font-size: 26rpx;
}
.voucher-ticket{
  position: relative;
  width: 704rpx;
  margin: 0 auto 23rpx;
}
.voucher-coupon-wrapper {
  border-radius: 12rpx;
  position: relative;
  overflow: hidden;
  -webkit-border-radius: 12rpx;
  -moz-border-radius: 12rpx;
  -ms-border-radius: 12rpx;
  -o-border-radius: 12rpx;
}
.voucher-coupon-top {
  display: flex;
  padding: 29rpx 31rpx 25rpx 40rpx;
  position: relative;
  overflow: hidden;
  background: #fff
}
.voucher-coupon-left{
  color: #FF5500;
  width: 104rpx;
  position: relative;
}
.voucher-coupon-center{
  padding-left: 45rpx;
}
.voucher-coupon-title{
  font-size: 30rpx;
  color: #333333;
  width: 360rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.voucher-coupon-des{
  font-size: 30rpx;
  width: 360rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.voucher-coupon-time{
  font-size: 22rpx;
  color: #999999;
}
.voucher-coupon-image{
  width: 104rpx;
  height:112rpx;
  margin-top: 5rpx;
}
.voucher-coupon-number{
  font-size: 34rpx;
  position: absolute;
  left: 0;
  top: 25rpx;
  width: 100%;
  text-align: center;
}
.voucher-coupon-number-unit{
  font-size: 22rpx;
}
.voucher-coupon-right{
  padding:0 24rpx;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  border-radius:26rpx;
  font-size: 24rpx;
  color: #ffffff;
  position: absolute;
  top: 62rpx;
  right: 31rpx;
}
.voucher-coupon-wrapper-center{
  position: relative;
  font-size: 0;
}
.voucher-coupon-round-left{
  width: 20rpx;
  height: 20rpx;
  background: #F3F3F3;
  border-radius: 50%;
  position: absolute;
  top: -10rpx;
  left: -10rpx;
}
.voucher-coupon-round-right{
  width: 20rpx;
  height: 20rpx;
  background: #F3F3F3;
  border-radius: 50%;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.voucher-coupon-bottom {
  font-size: 22rpx;
  color: #999999;
  padding: 20rpx 30rpx;
  text-align: left;
  background: #FAFAFA;
}
.voucher-coupon-bottom .ico-moon{
  float: right;
  line-height: 30rpx;
}
.voucher-coupon-des-item{
  line-height: 30rpx;
  width: 600rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.coupon-type-5{
  white-space:normal;
  word-break: break-all;
}
.new-coupon .voucher-coupon-top{
  padding: 36rpx 31rpx 50rpx 0;
}
.new-coupon .voucher-coupon-left{
  width: 204rpx;
  text-align: center;
}
.new-coupon .voucher-coupon-unit{
  font-size: 28rpx;
}
.new-coupon .voucher-coupon-number-new{
  font-size: 50rpx;
}
.new-coupon .voucher-coupon-type{
  font-size: 26rpx;
}
.new-coupon .voucher-coupon-center{
  padding-left: 0;
}
.new-coupon .voucher-coupon-type-new{
  font-size: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.new-coupon .voucher-coupon-title{
  width: 295rpx;
}
.voucher-verification-window{
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.5);
  position: fixed;
  top: 0;
  left: 0;
}
.voucher-verification-content{
  width: 560rpx;
  margin: 560rpx auto 0;
  border-radius:10rpx;
  background: #fff;
  padding-top: 37rpx;
}
.voucher-verification-title{
  text-align: center;
  font-size: 28rpx;
  font-weight: bold;
}
.voucher-verification-window input{
  width: 435rpx;
  background:#F5F5F5;
  height:76rpx;
  margin: 28rpx auto 0;
  padding: 10rpx 10rpx;
  box-sizing: border-box;
}
.voucher-verification-bottom{
  padding: 41rpx 84rpx;
  display: flex;
  justify-content: space-between;
}
.voucher-verification-botton{
  width: 186rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  border-radius:28rpx;
}
.cancel-botton{
  background: #D2D2D2;
}
.confirm-botton{
  background:linear-gradient(90deg,rgba(255,150,0,1) 0%,rgba(255,114,0,1) 100%);
}
.lose-efficacy-style{
  color:#CACACA !important;
}
.coupon-type-image{
  width: 160rpx;
  height: 121rpx;
  position: absolute;
  right: -16rpx;
  bottom: 10rpx;
}
.francheisee-voucher{
  position: relative;
  width: 708rpx;
  margin: 0 auto 20rpx;
  background: #ffffff;
  padding-bottom: 16rpx;
  box-sizing: border-box;
  border-radius: 14rpx;
}
.francheisee-voucher-store{
  width: 100%;
  height: 74rpx;
  line-height: 74rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx 0 15rpx;
  box-sizing: border-box;
}
.francheisee-voucher-store-left{
  display: flex;
  align-items: center;
}
.coupon-franchisee-logo{
  width:42rpx;
  height:42rpx;
  border-radius:4rpx;
  margin-right: 16rpx;
}
.many-businesses-icon{
  display: inline-block;
  width:64rpx;
  height:32rpx;
  line-height: 32rpx;
  background:rgba(255,77,100,0.1);
  text-align: center;
  color: #FF4D64;
  font-size: 24rpx;
  vertical-align: middle;
  margin-right: 8rpx;
}
.franchisee-store-name{
  font-size:28rpx;
  color: #333333;
  width: 300rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.francheisee-voucher-store-right{
  font-size:22rpx;
  color:rgba(153,153,153,1);
}
.francheisee-voucher-content{
  padding: 0 13rpx;
}
.francheisee-voucher .voucher-coupon-title,.francheisee-voucher .voucher-coupon-des{
  width: 275rpx
}
.francheisee-voucher .voucher-coupon-top{
  background: #FFFAF6;
  border-bottom: dashed 1rpx #FFD3D3;
}
.francheisee-voucher .voucher-coupon-bottom{
  background: #FFFAF6;
}
.francheisee-voucher .voucher-coupon-round-left,.francheisee-voucher .voucher-coupon-round-right{
  background: #ffffff
}
.maTop6{
  margin-top: 6rpx;
}
.maTop11{
  margin-top: 11rpx;
}
.maTop14{
  margin-top: 14rpx;
}
.coupon-verify-wrap-mark{
  display: none;
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0 , 0, 0.5);
  left: 0;
  top: 0;
  z-index: 5;
}
.coupon-verify-wrap {
  width: 586rpx;
  height: 736rpx;
  margin: 275rpx auto 0;
  background: #fff;
  border-radius: 12rpx;
  padding: 42rpx 45rpx 0;
  box-sizing: border-box;
}
.coupon-verify-wrap-mark.show {
  display: block;
}
.coupon-verify-wrap .top-section{
  border-bottom: 2rpx dashed #cccccc;
}
.coupon-verify-wrap .qrcode{
  width: 100%;
  height: 498rpx;
}
.coupon-verify-wrap .qrcode-tip{
  padding: 28rpx 0;
  font-size:26rpx;
  line-height: 30rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  text-align: center;
}
.coupon-verify-wrap .coupon-verification{
  height: 101rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}
.coupon-verify-wrap .coupon-verification-number{
  font-size: 34rpx;
  font-weight: bold;
  color:#333333;
}
.coupon-verify-wrap-mark .close-verify-wrap-mark{
  width: 55rpx;
  height: 55rpx;
  border-radius: 50%;
  border:2rpx solid rgba(255,255,255,1);
  text-align: center;
  line-height: 55rpx;
  color: #fff;
  font-size: 32rpx;
  margin: 43rpx auto 0;
}
.top-section-qrcode-block{
  width: 100%;
  height: 498rpx;
  position: relative;
}
.lose-qrcode-mark{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background:rgba(255,255,255,0.8);
}