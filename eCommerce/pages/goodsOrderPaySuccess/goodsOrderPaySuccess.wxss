page {
  height: 100%;
  background: #f3f3f3;
}
.normal-area {
  background: linear-gradient(to right,rgba(255,174,0,1),rgba(255,113,0,1));
  position: relative;
}
.normal-area .goods-complete-bg {
  width: 100%;
  height: 78rpx;
  display: block;
  position: absolute;
  bottom: -2rpx;
}
.normal-area .goods-complete-succes {
  width: 46rpx;
  height: 46rpx;
  display: inline-block;
  vertical-align: middle;
}
.order-success text{
  display: inline-block;
  vertical-align: middle;
  margin-left: 20rpx;
}
.collect-benefit-area {
  background: #fff;
  color:#818181;
}
.collect-item {
  position: relative;
  width: 100%;
  overflow:hidden;
}
.collect-item .star-num {
  margin: 30rpx 30rpx 50rpx 30rpx;
  font-size: 26rpx;
  color: #434343;
}
.collect-item .star-list {
  margin: 0 auto 10rpx;
  text-align: center;
  font-size: 0;
}
.collect-item .star-list .star-item {
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  margin-right: 60rpx;
  margin-bottom: 40rpx;
}
.collect-item .star-full {
  position: absolute;
  top: -30rpx;
  right: -30rpx;
  transform: rotate(-25deg);
  padding: 8rpx;
  border: 1px solid #fd888a;
  border-radius: 50%;
}
.collect-item .star-full-text {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 55rpx;
  text-align: center;
  color: #fd888a;
  border: 1px solid #fd888a;
  border-radius: 50%;
}
.tip-item {
  line-height: 37rpx;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #666;
}
.tip-item > view {
  line-height: 37rpx;
  border-top: 1rpx solid #f3f3f3;
  padding: 20rpx 0;
}
.btn-area{
  text-align: center;
  margin-top: 20rpx
}
.btn-area > view {
  width: 178rpx;
  line-height: 58rpx;
  display: inline-block;
  margin: 0 20rpx;
  font-size: 26rpx;
  text-align: center;
  border-radius: 30rpx;
  border: 1px solid #FFFFFF;
  color: #fff;
}
.scratchWhole{
  background:#ff7100;
  padding: 0 0 40rpx 0;
}
.scratchArea{
  width: 604rpx;
  height: 300rpx;
  margin: 0 auto;
  background-image: url(http://cdn.jisuapp.cn/static/webapp/images/scratchCon.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  position: relative;
  overflow: hidden;
}
.scratch_wp{
  margin: 19rpx auto 0;
  width: 558rpx; 
  height: 258rpx;
}
.scratch-canvas{
  background: transparent;
  position: relative;
}
.showArea{
  position: absolute;
  left:0;
  top: 0; 
  width: 556rpx;
  height: 263rpx;
  z-index: 5; 
}
.showArea .showTitle{
  width: 100%;
  font-size: 24rpx;
  color: rgb(102,102,102);
  text-align: center;
  padding: 60rpx 0;
  letter-spacing: 4rpx;
}
.showArea .showBtn{
  width: 206rpx;
  height: 70rpx;
  background: #ff5b3f;
  color: #fff;
  font-size: 24rpx;
  border-radius: 9rpx;
  text-align: center;
  line-height: 70rpx;
  margin: 0 auto;
}
.scratchFail {
  position: absolute;
  left: 0;
  top: 0;
  width: 604rpx;
  height: 300rpx;
}
.scratchFail image {
