.buyer-information {
  padding: 30rpx;
  background: #fff;
}
.buyer-name-box {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 6rpx;
}
.buyer-name-box > text {
  vertical-align: middle;
  display: inline-block;
}
.buyer-name {
  overflow: hidden;
  max-width: 300rpx;
  margin-right: 10rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.buyer-box {
  font-size: 26rpx;
  color: #666;
  line-height: 40rpx;
  width: 630rpx;
  vertical-align: middle;
  display: inline-block
}
.buyer-information .icon-goods-location {
  margin-right: 26rpx;
  font-size: 34rpx;
  vertical-align: middle;
}
.buyer-address {
  font-size: 26rpx;
  color: #666;
  text-overflow: ellipsis;
  display: -webkit-box; 
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  overflow: hidden;
}
.line{
  width: 120rpx;
  height: 1px;
  background:#ccc;
  display: inline-block;
  vertical-align: middle;
}
.activity-box {
  padding: 20rpx;
  text-align: center;
  background: #f3f3f3;
  color: #CCCCCC;
  font-size: 22rpx;
}
.activity-title {
  display: inline-block;
  vertical-align: middle;
  margin: 0 20rpx;
}
.order-price-box {
  padding: 0 30rpx;
  text-align: center;
  height: 372rpx;
}
.order-price {
  padding-top: 20rpx;
  color: #fff;
  font-size: 40rpx;
}
.order-success {
  color: #fff;
  font-size: 40rpx;
  padding-top: 50rpx;
}
.scratch-activity {
  margin-bottom: 40rpx;
}
.cash-delivery {
  line-height: 28rpx;
  width: 60rpx;
  color: #fff;
  background: #F2303E;
  font-size: 22rpx;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
  margin-right: 15rpx;
  border-radius: 4rpx;
}
.come-on {
  background: #ff7100;
}
.no-recommend-box {
  background: #fff;
}
.no-recommend-box .normal-area {
  height: 280rpx;
  text-align:center;
}
.no-recommend-box .btn-area {
  margin-top: 0;
  height: 130rpx;
}
.no-recommend-box .goods-complete-title {
  font-size: 54rpx;
  text-align: center;
  color: #ff7100;
  margin-top: 30rpx;
}
.no-recommend-box .slogan {
  text-align: center;
  color: #999;
  margin: 10rpx 0 40rpx 0;
  font-size: 26rpx;
}
.no-recommend-box .btn-area > view {
  line-height: 70rpx;
  font-size: 28rpx;
  width: 335rpx;
  border-radius: 35rpx;
  border-color: #d2d2d2;
  color: #666;
  margin: 0 10rpx;
}
.no-recommend-box .btn-area .go-on-shopping {
  border-color: #ff7100;
  color: #ff7100;
}
.no-recommend-box .goods-complete-succes {
  width: 125rpx;
  height: 125rpx;
  margin-top: 40rpx;
}
.settlement-activity{
  background-color: #fff;
  padding-top: 40rpx;
}
.settlement-activity .balance-item,
.settlement-activity .card-item {
  margin: 0 30rpx 0 30rpx;
  border-radius: 12rpx;
  padding: 30rpx;
  background-color: #FAFAFA;
}
.settlement-activity .balance-item .row {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.settlement-activity .balance-item .row:last-child {
  margin-bottom: 0;
}
.settlement-activity .balance-item .title,
.settlement-activity .balance-item .price {
  font-size: 30rpx;
  color: #333333;
}
.settlement-activity .balance-item .time {
  font-size: 24rpx;
  color: #999999;
}
.settlement-activity .balance-item .navigate {
  display: inline-flex;
  align-items: center;
  font-size: 22rpx;
  color: #FF7100;
}
.settlement-activity .balance-item .navigate .icon-rightarrow {
  margin-left: 4rpx;
  font-size: 14rpx;
}
.settlement-activity>.card-title {
  position: relative;
  left: 50%;
  transform: translate(-50%);
  margin: 0 0 30rpx 0;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
}
.settlement-activity>.card-title::before,
.settlement-activity>.card-title::after {
  content: '';
  position: absolute;
  width: 100rpx;
  height: 2rpx;
  background-color: #CCCCCC;
  top: 18rpx;
}
.settlement-activity>.card-title::before {
  left: 220rpx;
}
.settlement-activity>.card-title::after {
  right: 220rpx;
}
.settlement-activity .card-item {
  margin-top: 0;
  padding: 60rpx 30rpx 25rpx 20rpx;
}
.settlement-activity .card-detail {
  display: flex;
  position: relative;
  margin-bottom: 50rpx;
}
.settlement-activity .card-detail::before{
  content: '';
  position: absolute;
  top: 50px;
  left: -12px;
  width: 12px;
  height: 6px;
  border-radius: 6px 6px 0 0;
  border-width: 1px 1px 0 0;
  border-color: #FAFAFA;
  border-style: solid;
  transform: rotate(90deg);
  background: #FFF;
}
.settlement-activity .card-detail::after{
  content: '';
  position: absolute;
  top: 50px;
  right: -18px;
  width: 12px;
  height: 6px;
  border-radius: 6px 6px 0 0;
  border-width: 1px 1px 0 0;
  border-color: #FAFAFA;
  border-style: solid;
  transform: rotate(270deg);
  background: #FFF;
}
.settlement-activity .card-detail .cover-thumb{
  flex: 0 0 80rpx;
  height: 80rpx;
  margin-right: 28rpx;
  border-radius: 50%;
}
.settlement-activity .card-detail .title-warpper{
  flex: 0 0 auto;
}
.settlement-activity .card-detail .nickname{
  margin-bottom: 20rpx;
  line-height: 1;
  font-size: 30rpx;
  color: #333333;
}
.settlement-activity .card-detail .card-name{
  font-size: 26rpx;
  color: #999999;
}
.settlement-activity .card-detail .card-navigate{
  position: absolute;
  top: 10rpx;
  right: 0;
  width: 150rpx;
  line-height: 50rpx;
  text-align: center;
  border: 1px solid rgba(255,113,0,.4);
  border-radius: 25rpx;
  background-color: #FFF1E5;
  font-size: 26rpx;
  color: #FF7100;
}
.settlement-activity .card-item .row {
  display: flex;
  justify-content: space-between;
}
.settlement-activity .card-item .number, .settlement-activity .card-item .extra-date{
  font-size: 24rpx;
  color: #999999;
}
