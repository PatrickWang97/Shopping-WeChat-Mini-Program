  left: 0;
  top: 0;
}
.scratchFail .failTitle .failShare{
  font-size: 24rpx;
  color: #999;
  text-align: right;
  padding: 24rpx 40rpx;
  cursor: pointer;
  background: none;
  line-height: 1.5;
}
.scratchFail .failTitle .failText{
  text-align: center;
  margin-top: 55rpx;
  font-size: 32rpx;
  color: rgb(102,102,102);
}
.scratchFail .failTitle .failBtn{
  width: 206rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #ff5b3f;
  text-align: center;
  color: #fff;
  border-radius: 8rpx;
  margin: 0 auto;
  font-size: 24rpx;
  cursor: pointer;
}
.scratchWinning{
