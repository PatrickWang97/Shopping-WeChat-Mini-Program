page{
  background-color:#F3F3F3;
}
.newAddressView {
  border-bottom: 1px solid #f3f3f3;
}
.newAddressBox {
  background: #fff;
  padding: 10rpx 30rpx;
  font-size: 28rpx;
}
.newAddressBox > view {
  height: 94rpx;
  line-height: 94rpx;
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.newAddressBox input {
  line-height:94rpx;
  height:94rpx;
  vertical-align:middle;
  width: 520rpx;
  text-align:right;
  float:right;
}
.newAddressBox label {
  width: 124rpx;
  display: inline-block;
  vertical-align: top;
  color: #333;
}
.newAddressBox .adressLabel {
  display: inline-block;
  border: 1px solid #D5D5DE;
  color: #999;
  min-width: 60rpx;
  line-height: 46rpx;
  text-align: center;
  margin-left: 20rpx;
  border-radius: 24rpx;
  padding: 0 20rpx;
}
.newAddressBox .adressLabel.active {
  border: 1px solid #ff7100;
  color: #ff7100;
  background: #FFF1E5;
}
.province,
.city,
.dirstrict{
  line-height: 82rpx;
  height: 82rpx;
  text-indent: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
}
.province.active,
.city.active,
.dirstrict.active{
  color: #333;
}
.adress-label-box {
  text-align: right;
  float: right;
  margin-top: 2rpx;
}
.newAddressView .icon-rightarrow{
  float: right;
  font-size: 12px;
  color: #d5d5d5;
  margin-left: 10rpx
}
.nationName {
  width: calc( 100% - 160rpx);
  display: inline-block;
  text-align: right;
}
.nationName image{
  width: 52rpx;
  height: 36rpx;
  vertical-align: middle;
}
.address-pick-up {
  float: right;
  text-align: right;
}
.address-pick-up > view {
  display: inline-block;
  margin-left: 30rpx;
  color: #666;
}
.address-pick-up text {
  display: inline-block;
  vertical-align: middle;
}
.address-pick-up > view > .icon-goods-support-delivery {
  margin-right: 10rpx;
  font-size: 30rpx;
}
.address-pick-up > view .suporrt {
  color: #ff7100;
}
.area-code-picker {
  width: 500rpx;
  float: right;
  text-align: right;
}
.intelligent-address-mask {
  position: fixed;
  background: rgba(0,0,0,0.5);
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 9;
}
.intelligent-address-box {
  width: 560rpx;
  background: #fff;
  position: absolute;
  top: 164rpx;
  left: 50%;
  margin-left: -280rpx;
  border-radius: 10rpx;
  z-index: 10;
}
.intelligent-address-mask textarea{
  line-height: 48rpx;
  font-size: 28rpx;
  width: 440rpx;
  height: 260rpx;
  background: #F5F5F5;
  margin: 0 atuo;
  margin-left: 40rpx;
  padding: 14rpx 20rpx;
}
.intelligent-title {
  text-align: center;
  height: 108rpx;
  line-height: 108rpx;
  font-size: 30rpx;
  font-weight: 600;
}
.intelligent-btn-box {
  display: flex;
  border-top: 2rpx solid #EBEBEB;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 40rpx;
}
.intelligent-btn-box text {
  flex: 1;
  text-align: center;
  color: #666666;
  font-size: 30rpx;
}
.intelligent-btn-box text:last-child {
  border-left: 2rpx solid #EBEBEB;
  color: #3091f2;
}
.default-address{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.default-address label{
  width: 500rpx;
  line-height: 38rpx;
  padding-top: 16rpx;
}
.default-address .tip{
  color: #999999;
  font-size: 24rpx;
}
.default-address switch{
  transform: scale(0.7);
}
.btn-wrapper{
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
}
.primary-btn, .default-btn{
  width: 50%;
  line-height: 94rpx;
  text-align: center;
  font-size: 30rpx;
}
.primary-btn {
  background: #F71000;
  color: #fff;
}
.default-btn{
  background: #fff;
  color: #000;
}
.choose_address_modal {
  background: #fff;
  border-radius: 18rpx 18rpx 0 0;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999;
  height: 900rpx;
}
.choose_address_modal .content {
  padding: 0 30rpx;
  z-index: 99;
  min-height: 856rpx;
}
.choose_address_modal .title {
  color: #333;
  font-weight: bold;
  font-size: 34rpx;
  text-align: center;
  padding: 40rpx 0;
  position: relative;
}
.choose_address_modal .closes {
  position: absolute;
  right: 10rpx;
  top: 50rpx;
  color: #b6b6b6;
  font-size: 28rpx;
}
.choose_address_modal .region {
  display: inline-block;
  padding-bottom: 18rpx;
  margin-right: 42rpx;
}
.choose_address_modal .region.selected {
  border-bottom: 2rpx solid rgba(255, 113, 0, 1);
  font-weight: bold;
}
.choose_address_modal .tips {
  font-size: 24rpx;
  color: #999;
  padding: 20rpx 0;
}
.choose_address_modal .scroll_wrap {
  max-height: 600rpx;
}
.choose_address_modal .scroll_wrap2 {
  display: flex;
  justify-content: space-around;
  align-items: flex-start;
}
.choose_address_modal .scroll_item {
  margin-bottom: 62rpx;
}
.choose_address_modal .scroll_item_fname {
  color: #999; 
  font-size: 24rpx;
  display: inline-block;
  width: 24rpx;
}
.choose_address_modal .scroll_item_name {
  font-size: 28rpx;
  color: #333;
  margin-left: 30rpx;
}
.choose_address_modal .scroll_item_name.selected{
  color: #ff7100;
}
.choose_address_modal .scroll_item2 {
  padding: 16rpx 30rpx;
  border-radius: 32rpx;  
  font-size: 28rpx;
  color: #656565;
  background: #F3F3F3;
}
.choose_address_modal .scroll_item2.selected {
  background: #FFF1E5;
  border: 1rpx solid rgba(255, 131, 0, 1);
  color: #FF7100;
}
.choose_address_modal .address_wrap {
  padding: 30rpx 0 10rpx;
}
.choose_address_modal .address_text {
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  margin-right: 36rpx;
}
.choose_address_modal .address_text.selected {
  color: #ff7100;
}
.choose_address_modal .address_tip {
  font-size: 28rpx;
}
.choose_address_modal .address_tip.selected {
  color: #FF7100;
}