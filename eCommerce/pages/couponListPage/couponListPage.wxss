page {
    background: #efeff5;
}
.goods-coupon-list {
    width: 100%;
}
.franchisee-goods-coupon-container{
	position: relative;
	background-color: #efeff5;
	height: auto;
    width: 100%;
    padding-bottom: 30rpx;
}
.coupon_ticket{
  margin-top: 40rpx;
}
.coupon_ticket .ticket-wrap{
    height: 160rpx;
    width: 690rpx;
    margin: 0 auto;
    background: #fff;
    position: relative;
    overflow: hidden;
}
.coupon_ticket .ticket-wrap::before{
    content: '';
    position: absolute;
    top: -12rpx;
    left: 170rpx;
    border: 12rpx solid #efeff5;
    border-radius: 50%;
}
.coupon_ticket .ticket-wrap::after{
    content: '';
    position: absolute;
    bottom: -12rpx;
    left: 170rpx;
    border: 12rpx solid #efeff5;
    border-radius: 50%;
}
.coupon_ticket .ticket-wrap .price{
    float: left;
    height: 160rpx;
    width: 180rpx;
    text-align: center;
    line-height: 160rpx;
    background: #e66b67;
    color: #fff;
    font-size: 40rpx;
    border-right: 1px dashed #efeff5;
}
.coupon_ticket .ticket-wrap .coupon_right{
    float: left;
    margin-left: 30rpx;
    width: 470rpx;
}
.ticket-wrap .coupon_right .coupon_name{
    font-size: 32rpx;
    color: #000;
    font-weight: normal;
    margin: 26rpx 0 16rpx 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.ticket-wrap .coupon_right view{
    font-size: 24rpx;
    line-height: 40rpx;
    color: #b2b2b2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.ticket-wrap .coupon_right .coupon-time{
  font-size: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.ticket-wrap .coupon_right .coupon-league-mark {
    display: inline-block;
    position: relative;
    top: -2rpx;
    width: 64rpx;
    height: 30rpx;
    margin-right: 12rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    color: #e66b67;
}
.coupon-league-mark view{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    opacity: 0.1;
    background-color: #e66b67;
}