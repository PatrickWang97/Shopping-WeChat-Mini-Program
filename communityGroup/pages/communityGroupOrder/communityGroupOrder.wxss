page {
  height: 100%;
  background-color: #eee;
  font-size: 29rpx;
}
.hidden-modal {
  top:0;
  left:0;
  right:0;
  bottom:0;
  background:rgba(0, 0, 0, 0.5);
  z-index:3;
  position:fixed;
}
.order-status-pannel {
  padding: 0 26rpx;
  background: #fff;
}
.order-status-pannel > view:nth-child(1) {
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  border-bottom: 2rpx solid #F3F3F3;
}
.order-status-pannel > view:nth-child(1) text {
  color: #333333;
  font-size: 26rpx;
  font-weight: bold;
}
.order-status-pannel > view .icon-uparrow {
  color: #D2D2D2;
}
.order-status-pannel > view:nth-child(2) {
  display:flex;
  height:246rpx;
  justify-content:space-between;
  flex-wrap:wrap;
  padding-top:26rpx;
  padding-bottom:40rpx;
  align-content:space-between;
}
.order-status-pannel > view:nth-child(2) view {
  width:216rpx;
  height:66rpx;
  border-radius:8rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 66rpx;
  display: inline-block;
  background:#F3F3F3;
  color: #666666;
}
.order-status-pannel > view:nth-child(2) view.active-menu {
  background:rgba(255,113,0,1);
  color: #fff;
}
.myOrder-wrap .drop-down {
  height: 88rpx;
  width: 82rpx;
  position: fixed;
  z-index:2;
  top:0;
  right: 0;
  background-color:#fff; 
  line-height:88rpx;
  text-align:center;
  color:#D2D2D2;
  box-shadow:-2rpx -8rpx 20rpx rgba(48,52,69,0.2);
}
.myOrder-tab-list {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  line-height: 85rpx;
  height: 88rpx;
  font-size: 26rpx;
  background: #fff;
  text-align: center;
}
.myOrder-tab-list view {
  border-bottom: 2px solid transparent;
}
.myOrder-tab-list view.active {
  border-color: #ff7100;
  color: #ff7100;
}
.lengthOneBody {
  height: calc(100% - 94rpx);
  margin-top: 110rpx;
}
.scrollContainer {
  height: 100%;
}
.myOrder-list-item {
  background-color: #fff;
  margin-bottom: 10rpx;
}
.myOrder-list-title {
  height: 94rpx;
  margin-bottom: 20rpx;
  padding: 30rpx 30rpx 0;
  border-bottom: 1rpx solid #f3f3f3;
}
.myOrder-list-title .title-name {
  display: block;
  color: #333;
  font-size: 30rpx;
  margin-bottom: 10rpx;
}
.myOrder-list-title .title-time {
  display: block;
  font-size: 24rpx;
  color: #999;
  line-height:28rpx;
}
.myOrder-list-title .title-time text {
  float: right;
  color: #ff7100;
}
.myOrder-list-goods {
  margin-bottom: 7rpx;
}
.goods-item {
  height: 160rpx;
  padding: 14rpx 30rpx 0;
  background: #fafafa;
  margin-bottom: 7rpx;
}
.goods-img {
  float: left;
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
  margin-right: 42rpx;
  background-color: #fff;
  border-radius:4rpx;
}
.goods-img image {
  display: block;
  width: 100%;
  height: 100%;
}
.goods-describe {
  float: left;
  width: calc(100% - 182rpx);
  height: 150rpx;
}
.goods-describe .goods-describe-title, .goods-describe .goods-describe-price,
.goods-describe .goods-describe-model {
  display: block;
  color: #333;
  font-size: 24rpx;
}
.goods-describe .goods-describe-title {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.goods-describe .goods-describe-model {
  font-size: 22rpx;
  color: #999;
  margin: 10rpx 0 15rpx;
}
.goods-describe .goods-describe-price text {
  float: right;
}
.list-footer-btn {
  height: 80rpx;
  line-height: 80rpx;
  text-align: right;
  padding: 0 30rpx;
}
.list-footer-label {
  padding: 20rpx 30rpx 26rpx;
  font-size: 24rpx;
  color: #666;
  border-bottom: 1rpx solid #f3f3f3;
  text-align: right;
  line-height: 37rpx;
}
.list-footer-label .colorRed {
  color: #ff3600;
}
.list-footer-label .colorOrigin {
  color: #ff7100;
}
.list-footer-btn text {
  display: inline-block;
  width: 174rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 10rpx;
  border-radius: 25rpx;
  border: 1rpx solid #ff7100;
  text-align: center;
  color: #ff7100;
  font-size: 24rpx;
}
.list-footer-btn text.checkLogistic {
  display: inline-block;
  width: 174rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 10rpx;
  border-radius: 25rpx;
  border: 1rpx solid #D2D2D2;
  text-align: center;
  color: #666666;
  font-size: 24rpx;
}
.noMoreNewDataTip {
  text-align: center;
  color: #aaa;
  line-height: 30px;
  margin-bottom: 10px;
}
.tostore-list-submenu view.active {
  color: #ff7100;
  font-weight:bold;
}
.tostore-sub-box {
  display: inline-block;
  padding: 0 16rpx;
  margin: 0 10rpx;
}
.tostore-list-submenu {
  overflow-x: auto;
  white-space: nowrap;
  background: #fff;
  color: #666666;
  border-bottom: none;
  padding-right:82rpx;
}