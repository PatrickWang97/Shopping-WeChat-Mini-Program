page {
    background-color: #f3f3f3;
    padding-left: 24rpx;
    padding-right: 24rpx;
    box-sizing: border-box;
  }
  .cart-head {
    display: flex;
    align-items: center;
    width: 702rpx;
    height: 180rpx;
    background-color: #fff;
    margin-top: 24rpx;
    border-radius: 12rpx;
  }
  .cart-avatar {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    border: 6rpx solid #ffead9;
    min-width: 90rpx;
    margin: 46rpx 0 46rpx 36rpx;
  }
  .cart-detail-box {
    width: 524rpx;
    margin-left: 24rpx;
  }
  .cart-name {
    font-size: 30rpx;
    color: #303445;
    font-weight: bold;
    line-height: 30rpx;
    margin-bottom: 18rpx;
  }
  .cart-address {
    line-height: 26rpx;
    font-size: 26rpx;
    color: #666;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
  }
  .cart-goodsList.height {
    padding-bottom: 114rpx;
  }
  .cart-goodList {
    margin-top: 20rpx;
    border-radius: 12rpx;
    background-color: #fff;
  }
  .cart-goodList.height{
    margin-top: 20rpx;
    border-radius: 12rpx;
    background-color: #fff;
  }
  .cart-check-box {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    border: 2rpx solid #ebebeb;
    border-radius: 50%;
    background: #fff;
    box-sizing: border-box;
    line-height: 36rpx;
    margin-left: 30rpx;
  }
  .cart-check-box.checked {
    color: #ff7100;
    background: #fff;
    border: none;
    font-size: 40rpx;
  }
  .cart-good-conten {
    display: inline-block;
    vertical-align: middle;
    box-sizing: border-box;
    width: calc(100% - 70rpx);
    padding-left: 24rpx;
    padding-right: 30rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
  }
  .cart-good-cover {
    width: 160rpx;
    height: 160rpx;
    display: inline-block;
    vertical-align: top;
    border-radius: 8rpx;
    overflow: hidden;
  }
  .cart-good-cover image {
    width: 100%;
    height: 100%;
  }
  .good-content-box {
    display: inline-block;
    box-sizing: border-box;
    width: calc(100% - 160rpx);
    vertical-align: top;
    padding-left: 22rpx;
  }
  .good-title {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #333;
    font-weight: Regular;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }
  .good-detail{
    color: #999;
    font-size: 26rpx;
    line-height: 26rpx;
    display: inline-block;
    margin-right: 3rpx;
  }
  .cart-price-box{
    margin-top: 22rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  .real-price{
    color: #FF7100;
    font-size: 30rpx;
  }
  .good-number-box{
    display: flex;
    box-sizing: border-box;
    width: 170rpx;
    height: 62rpx;
    border-radius: 31rpx;
    border: 2rpx solid #F0F0F0;
    padding: 4rpx 6rpx;
  }
  .goods-num-sub{
    width: 50rpx;
    height: 50rpx;
    background-color: #F5F5F5;
    border-radius: 25rpx;
    display: inline-block;
    vertical-align: center;
    color: #BFBFBF;
    text-align: center;
    line-height: 45rpx;
  }
  .goods-num-add{
    width: 50rpx;
    height: 50rpx;
    background-color: #F5F5F5;
    border-radius: 25rpx;
    display: inline-block;
    vertical-align: center;
    color: #BFBFBF;
    text-align: center;
    line-height: 45rpx;
  }
  .goods-num-box{
    box-sizing: border-box;
    display: inline-block;
    vertical-align: center;
    margin: 0 5rpx;
    width: 50rpx;
    height: 50rpx;
    font-size: 24rpx;
    text-align: center;
  }
  .cart-good-line {
    width: 642rpx;
    height: 2rpx;
    background-color: #f3f3f3;
    margin-left: 30rpx;
  }
  .cart-failure-goodsList {
    width: 702rpx;
    margin-top: 20rpx;
    border-radius: 12rpx;
    background-color: #fff;
    margin-bottom: 114rpx;
  }
  .cart-failure-title {
    height: 64rpx;
    color: #333;
    font-size: 28rpx;
    font-weight: Regular;
    padding-top: 30rpx;
    padding-left: 28rpx;
  }
  .cart-failure-good-line {
    width: 662rpx;
    height: 2rpx;
    background-color: #f3f3f3;
    margin-left: 20rpx;
  }
  .cart-failure-good {
    height: 220rpx;
  }
  .cart-calculator {
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: fixed;
    left: 0;
    bottom: 0;
    height: 94rpx;
    background-color: #fff;
    padding-right: 24rpx;
    right: 0;
  }
  .cart-checkAll {
    font-weight: Regular;
    color: #666;
    font-size: 28rpx;
  }
  .cart-goods-total-price {
    font-weight: Regular;
    color: #333;
    display: flex;
    align-items: center;
    font-size: 30rpx;
  }
  .price-count {
    font-family: MicrosoftYaHei-Bold;
    font-size: 32rpx;
    font-weight: bold;
    color: #ff7100;
  }
  .cart-cal-button {
    width: 200rpx;
    height: 74rpx;
    background: linear-gradient(to right, #ff9600, #ff7200);
    border-radius: 38rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-left: 24rpx;
  }
  .cal-button {
    color: #fff;
    font-size: 30rpx;
  }
  .shoppingCart-null {
    text-align: center;
    margin-bottom: 80rpx;
  }
  .shoppingCart-null image {
    width: 200rpx;
    display: inline-block;
    height: 200rpx;
    margin: 120rpx 0 30rpx 0;
  }
  .shoppingCart-null-label {
    color: #666;
    font-size: 26rpx;
  }
  .go-home-page {
    width: 220rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
    background: #ff7100;
    border-radius: 30rpx;
    font-size: 28rpx;
    display: inline-block;
    margin-top: 50rpx;
  }