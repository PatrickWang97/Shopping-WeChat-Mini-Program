page {
  background-color: #f4f4f4;
}
.menu-list {
  display: flex;
  background: #fff;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.menu-list view {
  flex: 1;
  text-align: center;
  position: relative;
}
.menu-list .active {
  color: #ff7100;
}
.goods-search {
  padding: 20rpx 30rpx;
  background: #fff;
}
.goods-search input{
  display: inline-block;
  vertical-align: middle;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
}
.goods-search .icon-search {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 20rpx 0 30rpx;
  vertical-align: middle;
  color: #B3B3B3;
  font-size: 28rpx;
}
.goods-search-contain {
  background: #F0F0F0;
  border-radius: 8rpx;
}
.goods-contain {
  margin-top: 10rpx;
  padding: 0 30rpx;
  background: #fff;
}
.goods-content {
  padding: 30rpx 0;
  border-bottom: 1px solid #F3F3F3;
}
.goods-content image {
  width: 140rpx;
  height: 140rpx;
  display: inline-block;
  vertical-align: middle;
}
.goods-detail {
  display: inline-block;
  vertical-align: middle;
  margin-left: 20rpx;
  width:calc(100% - 160rpx);
}
.goods-title {
  color: #333;
  font-weight: bold;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.people-number {
  font-size: 26rpx;
  color: #999;
  margin-bottom: 25rpx;
}
.people-number text {
  color: #ff7100;
}
.withdraw-proportion {
  color: #999;
  font-size: 24rpx;
}
.withdraw-proportion > text {
  display: inline-block;
  vertical-align: middle;
}
withdraw-proportion > text {
  display: inline-block;
  vertical-align: middle;
}
.goods-price {
  color: #FF7100;
  font-size: 26rpx;
}
.withdraw-proportion-line {
  background:#ccc;
  margin: 0 20rpx;
  height: 18rpx;
  width: 1px;
}
.menu-list > view.active::after {
  content: '';
  display: inline-block;
  width: 100rpx;
  height: 1px;
  background: #ff7100;
  position: absolute;
  left: 50%;
  margin-left: -50rpx;
  bottom: 0;
}
.promotion-tab {
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  height: 88rpx;
  margin-bottom: 16rpx;
  background-color: #fff;
  text-align: center;
  line-height: 88rpx;
  font-size: 28rpx;
}
.promotion-tab .tab-item {
  -webkit-flex: auto;
  flex: auto;
}
.promotion-tab .tab-active {
  color: #ff7100;
  border-bottom: 1rpx solid #ff7100;
}
.community-goods {
  margin-top: 20rpx;
  padding: 0 30rpx;
}
.community-goods-item {
  position: relative;
  height: 170rpx;
  padding: 20rpx 80rpx 0 40rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
}
.community-goods-title {
  color: #333;
  font-size: 34rpx;
  margin-bottom: 10rpx;
}
.community-goods-label {
  font-size: 26rpx;
  margin-bottom: 20rpx;
  color: #666666;
}
.community-goods-label label:nth-child(2) {
  margin-left: 16rpx;
  color: #ff7100;
}
.community-goods-time {
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 21rpx;
  color: #ff7100;
  background-color: #FFF1E5;
  border-radius: 17rpx;
  font-size: 22rpx;
  white-space: nowrap;
}
.list-item-icon {
  position: absolute;
  right: 0;
  top: 12rpx;
}
.community-end .community-goods-time {
  color: #999;
  background-color: #f0f0f0;
}
.noMore {
  text-align: center;
  color: #aaa;
  line-height: 30px;
  margin-bottom: 10px;
}
.tabbar {
  width: 100%;
  display: flex;
  position: relative;
}
.tabbar .title {
  height: 102rpx;
  width: 290rpx;
  background: rgba(255,255,255,1);
  color: #333333;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 102rpx;
  text-align: center;
  position: relative;
}
.tabbar .active {
  height: 102rpx;
  width: 290rpx;
  color: #FF7100;
  font-size: 30rpx;
  font-weight: bold;
  line-height: 102rpx;
}
.tabbar .active::after {
  content: "";
  width: 70rpx;
  height: 4rpx;
  border-top: 4rpx solid #FF7100;
  position: absolute;
  left: 50%;
  border-radius: 2rpx;
  transform: translateX(-50%);
  bottom: 0;
}
.tabbar .filter {
  height: 102rpx;
  width: 170rpx;
  background: rgba(255,255,255,1);
  font-size: 28rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height: 102rpx;
  text-align: center;
}
.filter-items {
  position: absolute;
  height: 151rpx;
  width: 136rpx;
  background-color: #0F0F0F;
  border-radius: 10rpx;
  right: 20rpx;
  top: 102rpx;
  z-index: 1;
}
.select-triangle {
  display: inline-block;
  position: absolute;
  width: 24rpx;
  height: 11rpx;
  color: #0F0F0F;
  top: 78rpx;
  right: 108rpx;
}
.filter-items .item {
  color: white;
