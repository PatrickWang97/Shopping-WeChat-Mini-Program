page {
  background-color: rgb(243, 243, 243);
}
.apply-status {
  height: 180rpx;
  line-height: 180rpx;
  color: #fff;
  background: linear-gradient(90deg, rgb(255, 174, 0) 0%, #ff7100 100%);
  background-size: 100% 100%;
  font-size: 34rpx;
}
.apply-status label {
  margin-right: 20rpx;
}
.apply-status>view {
  padding-left: 36rpx;
  background-image: url('http://zhichiwangluo.com/zhichi_frontend/static/webapp/images/xcx-goods/goods-order-bg.png');
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 100%;
}
.apply-content {
  background-color: #fff;
}
.apply-list {
  background-color: #eee;
  font-size: 32rpx;
}
.apply-list-item {
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  -webkit-justify-content: center;
  justify-content: space-between;
  padding-right: 30rpx;
  background-color: #fff;
  line-height: 84rpx;
  border-bottom: 1rpx solid rgb(235, 235, 235);
  font-size:28rpx;
  color:#333;
  padding-left:30rpx;
}
.apply-list-item>view:last-child {
  max-width: 520rpx;
  text-overflow:ellipsis;
  overflow:hidden;
  white-space:nowrap;
}
.apply-list>.apply-list-item:last-child {
  border: none;
}
.apply-list-item .list-item-label {
  -webkit-flex-basis: 170rpx;
  flex-basis: 170rpx;
  overflow: hidden;
}
.apply-list-item .list-item-input {
  flex: auto;
  color: #666;
  text-align: right;
}
.apply-list-item .list-item-img {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  overflow: hidden;
}
.apply-list-item .list-item-img image {
  display: block;
  width: 100%;
  height: 100%;
}
.apply-list-item .list-item-icon {
  flex: auto;
  text-align: right;
  color: rgb(153, 153, 153);
  font-size: 26rpx;
}
.apply-refound {
  display: -webkit-flex;
  display: flex;
  background-color: #fff;
  padding: 28rpx 30rpx 28rpx 0;
  border-top: 1rpx solid rgb(235, 235, 235);
  padding-left:30rpx;
}
.apply-refound .list-item-label {
  -webkit-flex-basis: 224rpx;
  flex-basis: 224rpx;
  color: #333;
  font-size: 28rpx;
}
.apply-refound .list-item-textarea {
  width: calc(100% - 224rpx);
  max-height: 148rpx;
  color: #666;
  line-height: 37rpx;
  overflow-y: scroll;
  overflow-x: hidden;
  font-size: 28rpx;
  word-break: break-all;
  text-align: right;
}
.apply-explain {
  text-align: center;
  padding: 0 30rpx;
  margin-top: 32rpx;
  color: #ff7100;
  font-size: 24rpx;
}
.origin-btn {
  height: 124rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
}
.origin-btn button[type=default] {
  width: 710rpx;
  margin: 18rpx auto;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30rpx;
  border-radius: 6rpx;
  color: #fff;
  background-color: #ff7100;
  text-align: center;
}
.apply-info {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 10;
}
.apply-info .apply-info-message {
  width: 560rpx;
  padding-top: 43rpx;
  background-color: #fff;
  border-radius: 10rpx;
  text-align: center;
  margin: 230rpx auto 0;
}
.apply-info-message .message-title {
  line-height: 44rpx;
  color: #000;
  font-size: 30rpx;
  font-weight:bold;
}
.apply-info-message .message-text {
  padding: 0 30rpx;
  max-height: 350rpx;
  line-height: 44rpx;
  overflow-y: scroll;
  color: #333;
  text-align: left;
  margin-bottom: 50rpx;
  margin-top: 30rpx;
  word-break: break-all;
  font-size: 26rpx;
}
.apply-info-message .block-text-title {
  padding:0 30rpx;
  line-height:44rpx;
  color:#333;
  text-align:center;
  margin-top:30rpx;
  word-break:break-all;
  font-size:28rpx;
}
.apply-info-message .block-text {
  padding:0 30rpx;
  max-height:350rpx;
  line-height:44rpx;
  overflow-y:scroll;
  color:#333;
  text-align:center;
  margin-bottom:30rpx;
  word-break:break-all;
  font-size:28rpx;
}
.apply-info-message .block-show {
  font-size: 22rpx;
  padding: 0 30rpx;
  text-align: left;
  color:#333;
}
.apply-info-message .message-btn {
  line-height: 79rpx;
  border-top: 1rpx solid #ebebeb;
  color: #ff7100;
  font-size: 30rpx;
}
.apply-list-item-auto {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  padding: 25rpx 30rpx 25rpx 30rpx;
  color: #333;
  background-color: #fff;
  text-align: right;
  font-size: 28rpx;
}
.apply-list-item-auto view:nth-child(2) {
  width: calc(100% - 170rpx);
  word-break: break-all;
}
.delivery-model {
  margin-left: 20rpx;
}
.color-666 {
  color: #666;
}
checkbox {
  width: 40rpx;
  height: 60rpx;
}
checkbox .wx-checkbox-input {
  width: 25rpx;
  height: 25rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: #FF7100;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.extra-info-box text {
  padding-left: 30rpx;
  line-height: 80rpx;
  color: #666666;
  font-size: 28rpx;
}