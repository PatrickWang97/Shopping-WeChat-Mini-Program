.four-three-mode .price-sign-left {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.four-three-mode .price-sign-left-box {
  display: flex;
  flex-direction: column;
  justify-items: center;
  align-items: center;
  background: #FFEA73;
  border-radius: 8rpx 2rpx 2rpx 12rpx;
  height: 66rpx;
  width: 72rpx;
}
.four-three-mode .commodity-font {
  font-size: 20rpx;
  color: #ff7100;
  padding: 5rpx 10rpx;
  text-align: center;
  background: rgb(255 113 0 / 0.1);
  display: inline;
  border-radius: 30rpx;
}
.four-three-mode .commodity {
}
.four-three-mode .price-sign-left text {
  line-height: 30rpx;
  height: 30rpx;
}
.four-three-mode .price-sign-left text:first-child {
  line-height: 40rpx;
  height: 35rpx;
}
.four-three-mode .commodity-sign-box .df {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 90rpx;
  min-width: 135rpx;
  max-width: 200rpx;
  font-weight: bold;
  padding: 0 10rpx;
}
.four-three-mode .commodity-sign-box .df .fz10 {
  font-size: 28rpx;
  font-weight: 500;
}
.four-three-mode .commodity-sign-box .df .real-price {
  color: #8D4C0F;
  text-align: center;
  line-height: 38rpx;
  height: 38rpx;
  font-size: 28rpx;
}
.four-three-mode .commodity-sign-box .df .mgt10 {
  margin-top: 10rpx;
}
.four-three-mode-border {
  border-radius: 16rpx;
}
.four-three-mode .title {
  margin-top: 6px;
  font-weight: 700;
}
.four-three-mode .commodity-sign-box .df .vitrual-price {
  text-align: center;
  line-height: 40rpx;
  height: 40rpx;
  font-weight: 500;
  font-size: 24rpx;
  text-decoration: line-through;
}
.four-three-mode .vitrual-price {
  font-size: 24rpx;
  color: #999;
  text-decoration: line-through;
  margin-right: 14rpx;
}
.four-three-mode .purchase-btn {
  width: 140rpx;
  text-align: center;
  background: linear-gradient(45deg, #FF7200, #FF4600);
  height: 50rpx;
  line-height: 50rpx;
  color: #fff;
  font-size: 24rpx;
  border-radius: 60rpx;
}
