.tcommunity-topic-link {
  display: flex;
  align-items: center;
  margin-top: 10rpx;
  padding: 14rpx;
  box-sizing: border-box;
  height: 100rpx;
  background-color: #F5F5F5;
}
.tcommunity-topic-video {
  position: relative;
  height: 360rpx;
  overflow: hidden;
}
.tcommunity-topic-link .link-logo-wrap {
  position: relative;
  margin-right: 15rpx;
}
.tcommunity-topic-link .link-logo {
  display: block;
  width: 72rpx;
  height: 72rpx;
}
.tcommunity-topic-link .icon-news-video-play,
.tcommunity-topic-video .icon-news-video-play {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: rgba(0,0,0,.5);
  transform: translateX(-50%) translateY(-50%);
  border-radius: 50%;
}
.tcommunity-topic-video .icon-news-video-play {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 25rpx;
}
.tcommunity-topic-video .video-cover {
  display: block;
  width: 100%;
  height: 100%;
}
.tcommunity-topic-link .link-text {
  flex: 1;
  overflow: hidden;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 26rpx;
  color: #666;
}
.loading-wrap{
  height: 50rpx;
  padding: 50rpx 0;
}
.loading-view {
  margin: 0 auto;
  width: 50rpx;
  height: 50rpx;
  position: relative;
  text-align: center;
  -webkit-animation: rotate 2.0s infinite linear;
  animation: rotate 2.0s infinite linear;
}
.loading-dot1, .loading-dot2 {
  width: 50%;
  height: 50%;
  display: inline-block;
  position: absolute;
  top: 0;
  left: 20%;
  background-color: #F66087;
  border-radius: 100%;
  -webkit-animation: bounce 2.0s infinite ease-in-out;
  animation: bounce 2.0s infinite ease-in-out;
}
.loading-dot2 {
  top: auto;
  bottom: 0rpx;
  background-color: #65B6F5;
  -webkit-animation-delay: -1.0s;
  animation-delay: -1.0s;
}
@-webkit-keyframes rotate { 100% { -webkit-transform: rotate(360deg) }}
@keyframes rotate { 100% { transform: rotate(360deg); -webkit-transform: rotate(360deg) }}
@-webkit-keyframes bounce {
  0%, 100% { -webkit-transform: scale(0.0) }
  50% { -webkit-transform: scale(1.0) }
}
@keyframes bounce {
  0%, 100% {
    transform: scale(0.0);
    -webkit-transform: scale(0.0);
  } 50% {
    transform: scale(1.0);
    -webkit-transform: scale(1.0);
  }
}
