    align-items: baseline;
    margin: 10rpx 0;
}
.wxParse-li-text{
    align-items: center;
    line-height: 20px;
}
.wxParse-li-circle{
    display: inline-flex;
    width: 5px;
    height: 5px;
    background-color: #333;
    margin-right: 5px;
}
.wxParse-li-square{
    display: inline-flex;
    width: 10rpx;
    height: 10rpx;
    background-color: #333;
    margin-right: 5px;
}
.wxParse-li-ring{
    display: inline-flex;
    width: 10rpx;
    height: 10rpx;
    border: 2rpx solid #333;
    border-radius: 50%;
    background-color: #fff;
    margin-right: 5px;
}
.wxParse-u {
  text-decoration: underline;
}
.wxParse-hide{
    display: none;
}
.WxEmojiView{
    align-items: center;
}
.wxEmoji{
    width: 16px;
    height:16px;
}
.wxParse-table{
  display: table;
  width: 100% !important;
  border-collapse: collapse;
}
.table-box.wxParse-table{
  table-layout:fixed;
}
.wxParse-tbody{
  display: table-row-group;
  vertical-align: middle;
}
.wxParse-tr{
  display: table-row;
  vertical-align: middle;
}
.wxParse-th,
.wxParse-td{
  display: table-cell;
  padding:5px;
  font-size:28rpx;
  border: 1px solid #e0e0e0;
  word-break: break-all;
  vertical-align: middle;
}
.wxParse-th{
  background:#f0f0f0;
}
.wxParse-caption{
  display: table-caption;
  text-align: center;
  border: 1px dashed #e0e0e0;
}
.wxParse-text {
  white-space: pre-wrap;
}
.wxParse-br {
  height: 22px;
}
.wxParse-p {
  white-space:pre-wrap;
  max-width: 100%!important;
}
.wxParse-hr {
  margin: 5px 0;
  border-style: inset;
  border-width: 1px;
}
.wxParse-mpvoice audio {
  margin: 10px 0;
  display: block;
  width: 100%;
}
.wxParse-article {
  max-width: 100%!important;
}
