#myMessage {
  min-height: 100vh;
  background-color: #edeeef;
}
#myMessage .myMessage-top-nav {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  z-index: 9;
  background-color: #fff;
}
#myMessage .myMessage-type-list{
  display: flex;
  justify-content: space-around;
  background: #fff;
  font-size: 28rpx;
  padding: 0 30rpx;
}
#myMessage .myMessage-type-item {
  position: relative;
  min-width: 70px;
}
#myMessage .active-line{
  width: 68rpx;
  height: 4rpx;
  background: #3B97EF;
  margin: 0 auto;
}
#myMessage .myMessage-type-item.has-noread::after {
  position: absolute;
  top: 8px;
  right: -5px;
  width: 5px;
  height: 5px;
  background: #ff0000;
  border-radius: 50%;
}
#myMessage .myMessage-type-item.active.has-noread::after {
  display:none;
}
#myMessage .myMessage-type-item:last-child {
  margin-right: 0;
}
#myMessage .myMessage-type-item.active {
  color: #3B97EF;
}
#myMessage .myMessage-content-container {
  padding-top: 42px;
}
#myMessage .myMessage-content {
  display: none;
  height: calc(100vh - 42px);
}
#myMessage .myMessage-content.active {
  display: block;
}
#myMessage .message-item {
  margin: 16px 10px 0;
  padding: 10px 12px;  
  border-radius: 4px;
  background: #feffff;
}
#myMessage .message-item:last-child {
  margin-bottom: 16px; 
}
#myMessage .message-item .message-row {
  display: flex;
  padding-bottom: 16rpx;
}
#myMessage .message-item .message-row:last-child {
  padding-bottom: 0;
}
#myMessage .message-item .message-title {
  width: 540rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
#myMessage .message-item.type-pay .message-title ,
#myMessage .message-item.type-form .message-title ,
#myMessage .message-item.type-comment .message-title ,
#myMessage .message-item.type-Administrators .message-title{
  color: #55b737;
}
#myMessage .message-item .message-time {
  flex: 1;
  font-size: 12px;
  color: #acacac;
  text-align: right;
}
#myMessage .message-item .message-img {
  display: block;
  width: 50px;
  height: 50px;
}
#myMessage .message-item.type-pay .message-img ,
#myMessage .message-item.type-form .message-img ,
#myMessage .message-item.type-comment .message-img ,
#myMessage .message-item.type-Administrators .message-img{
  border-radius: 50%;
  color: #fff;
  text-align: center;
  font-size: 30px;
}
#myMessage .message-item.type-pay .message-img {
  background: #f6ce59;
}
#myMessage .message-item.type-form .message-img {
  background: #9dd64b;
}
#myMessage .message-item.type-comment .message-img {
  background: #f5ce59;
}
#myMessage .message-item.type-Administrators .message-img {
  background: #adadad;
}
#myMessage .message-item.type-pay .icon-red-envelope-block,
#myMessage .message-item.type-Administrators .icon-notify {
  display: inline-block;
  vertical-align: top;
  margin-top: 6px;
}
#myMessage .message-item.type-form .icon-message-form ,
#myMessage .message-item.type-comment .icon-franchisee-dynamics {
  display: inline-block;
  vertical-align: top;
  margin-top: 10px;
}
#myMessage .message-item .message-content {
  flex: 1;
  font-size: 14px;
  word-break: break-all;
  padding-left: 30rpx;
}
#myMessage .message-item.type-pay .message-price {
  margin-top: 6px;
}
#myMessage .message-item.type-pay .message-orderNum {
  margin-top: 4px;
  color: #acacac;
}
#myMessage .message-item.type-pay .message-price-text ,
#myMessage .message-item.type-pay .message-orderNum-text {
  display: inline-block;
}
#myMessage .myMessage-detail-form {
}
#myMessage .myMessage-form-list {
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  margin-top: 15px;
  background: #feffff;
}
#myMessage .myMessage-form-item {
  min-height: 36px;
  border-bottom: 1px solid #ddd;
  margin: 0 10px;
  padding: 5px 0;
  line-height: 36px;
}
#myMessage .myMessage-form-item:last-child {
  border: 0;
}
#myMessage .myMessage-form-item .data-name {
  display: inline-block;
  height: 36px;
  width: 170rpx;
  margin-right: 8px;
  vertical-align: top;
  color: #878889;
  text-align: center;
  overflow: hidden;
}
#myMessage .myMessage-form-item .data-content {
  display: inline-block;
  width: 500rpx;
  min-height: 36px;
  line-height: 36px;
  overflow: hidden;
  vertical-align: top;
  word-break: break-all;
}
#myMessage .myMessage-form-item .data-img {
  display: inline-block;
  width: 60px;
  height: 60px;
}
#myMessage .myMessage-detail-form .myMessage-form-confirm {
  margin: 14px 10px;
  padding: 10px 0;
  background: #55b737;
  border-radius: 5px;
  color: #fff;
  text-align: center;
  font-size: 16px;
}
.success-examine {
  color: #01BD31
}
.faile-examine {
  color: #FF3838;
}
.video-item {
  display: inline-block;
  width: 240rpx;
  height: 240rpx;
  margin-right: 10rpx;
}
.video-item .video {
  width: 100%;
  height: 100%;
}
.video-item .controls {
  position: relative;
  height: 100%;
  z-index: 10;
}
#myMessage-push-message.myMessage-content{
  padding-top: 16rpx;
}
#myMessage-push-message .message-item-push{
  margin: 16rpx auto;
  width:710rpx;
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  padding: 40rpx 47rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
#myMessage-push-message .message-content-push{
  width: 476rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  color:#333333;
  word-break: break-all;
}
#myMessage-push-message .message-time-push{
  font-size: 28rpx;
  text-align: right;
  color: #999999;
}