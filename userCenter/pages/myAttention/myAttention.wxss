page {
  height: 100%;
  background: #f3f3f3;
}
.attention {
  background: #f3f3f3;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.attention-item {
  border-radius: 10rpx;
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  margin-bottom: 20rpx;
}
.attention-item-left {
  display: flex;
}
.attention-item-pic {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  border-radius: 6rpx;
}
.attention-item-pic image {
  display: block;
  width: 100%;
  height: 100%;
}
.attention-div {
  padding-left: 24rpx;
  width: 350rpx;
}
.attention-item-name {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 1;
}
.attention-item-price {
  font-size: 24rpx;
  color: #666;
  padding: 18rpx 0;
}
.attention-item-industry {
  display: inline-block;
  padding: 4rpx 10rpx;
  background:rgba(255,59,73,.1);
  color: #FF3B49;
  font-size: 24rpx;
  border-radius: 6rpx;
  max-width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.attention-item-right {
  width: 100rpx;
  border-radius: 10rpx;
  overflow: hidden;
  font-size: 24rpx;
  line-height: 46rpx;
  text-align: center;
}
.franchisee-attention-no {
  background:rgba(255,59,73,1);
  color: #fff;
}
.franchisee-attention-has {
  color: #FF3B49;
  border:1px solid rgba(255,59,73,1);
}
.attention-none {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: -100rpx;
}
.attention-none image {
  width: 300rpx;
  height: 300rpx;
} 
.tips {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #999;
}