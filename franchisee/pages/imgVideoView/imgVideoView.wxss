.page {
  height: 100vh;
  background-color: #000;
}
.navigator-bar {
  height: 40px;
  color: #fff;
}
.navigator-bar .back-btn {
  padding: 20rpx;
  font-size: 30rpx;
  line-height: 40rpx;
}
.main video {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 100%;
  height: 500rpx;
}
.video-mask {
  height: 420rpx;
}
.video-mask .left-top-exit {
  width: 160rpx;
  height: 160rpx;
  opacity: 0;
}
.preview-item {
  position: relative;
  display: inline-block;
  width: 100%;
}
.preview-item .play-icon {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 68rpx;
  height: 68rpx;
  background-color: rgba(0, 0, 0, .5);
  border-radius: 34rpx;
}
.preview-item .play-icon image {
  width: 100%;
  height: 100%;
}
.back-home-btn {
  position: fixed;
  right: 100rpx;
  bottom: 100rpx;
  width:80rpx;
  height:80rpx;
  text-align:center;
  line-height:80rpx;
  background:rgba(0,0,0,0.5);
  border-radius:40rpx;
  font-size:40rpx;
  color: #fff;
}
