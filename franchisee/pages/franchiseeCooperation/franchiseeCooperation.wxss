page {
  background-color: #F3F3F3;
  height: 100%;
}
.franchisee-cooperation{
  height: 100%;
}
.franchisee-swiper image{
  width: 100%;
  height: 100%;
}
.franchiseeCoo-banner-default{
  display: block;
  width: 100%;
  height: 300rpx;
}
.franchiseeCoo-section{
  margin-top: 20rpx;
  background-color: #fff;
}
.franchiseeCoo-title{
  text-align: center;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  line-height: 90rpx;
}
.franchiseeCoo-title-icon{
  display: inline-block;
  width: 46rpx;
  height: 10rpx;
  background-size: 100% 100%;
  vertical-align: middle;
}
.franchiseeCoo-title-icon-left{
  background-image: url(http://cdn.jisuapp.cn/static/webapp/images/franchisee/coo-title-left.png);
  margin-right: 10rpx;
}
.franchiseeCoo-title-icon-right{
  background-image: url(http://cdn.jisuapp.cn/static/webapp/images/franchisee/coo-title-right.png);
  margin-left: 10rpx;
}
.franchiseeCoo-adv-content{
  padding-bottom: 40rpx;
}
.franchiseeCoo-adv-item{
  display: inline-block;
  width: 50%;
  font-size: 26rpx;
  text-align: center;
  margin-top: 20rpx;
   margin-bottom: 20rpx; 
}
.franchiseeCoo-adv-icon{
  width: 160rpx;
  height: 160rpx;
}
.franchiseeCoo-adv-text1{
  color: #333;
}
.franchiseeCoo-adv-text2{
  color: #9FA5BB;
}
.franchiseeCoo-agent{
  background-image: url(http://cdn.jisuapp.cn/static/webapp/images/franchisee/agent-bg.png);
  background-repeat: no-repeat;
  background-size: cover;
}
.franchiseeCoo-agent-content{
  font-size: 26rpx;
  color: #333;
  padding: 20rpx 30rpx;
}
.franchiseeCoo-agent-li{
  margin-bottom: 40rpx;
}
.franchiseeCoo-agent-lil{
  display: inline-block;
  color: #666;
  width: 170rpx;
  vertical-align: top;
}
.franchiseeCoo-agent-lir{
  display: inline-block;
  width: 520rpx;
}
.franchiseeCoo-agent-text{
  display: inline-block;
  width: 490rpx;
  vertical-align: middle;
}
.franchiseeCoo-agent-icon{
  display: inline-block;
  width: 30rpx;
  color: #3091F2;
  text-align: center;
  vertical-align: middle;
}
.franchiseeCoo-cooperation{
  background-image: url(http://cdn.jisuapp.cn/static/webapp/images/franchisee/coo-bg.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position: left bottom;
}
.franchiseeCoo-form-wrap{
  padding: 30rpx 30rpx 0;
}
.form-item{
  margin-bottom: 40rpx;
}
.form-title{
  display: inline-block;
  width: 110rpx;
  margin-right: 20rpx;
  color: #333;
  font-size: 26rpx;
  line-height: 70rpx;
  vertical-align: top;
}
.form-item-right{
  display: inline-block;
  width: 560rpx;
}
.form-input{
  display: block;
  height: 70rpx;
  padding: 0 10rpx;
  border: 1px solid #D5D5DE;
  box-sizing: border-box;
}
.form-phone{
  display: inline-block;
  width: 350rpx;
  vertical-align: middle;
}
.form-phone-get{
  display: inline-block;
  width: 180rpx;
  height: 70rpx;
  margin-left: 20rpx;
  color: #fff;
  font-size: 26rpx;
  background-color: #FF7100;
  border-radius: 4rpx;
  vertical-align: middle;
}
.form-btn{
  width: 100%;
  height: 80rpx;
  padding: 0;
  color: #fff;
  font-size: 30rpx;
  background-color: #FF7100;
  border-radius: 4rpx;
}
.input-placeholder,
.textarea-placeholder{
  font-size: 28rpx;
  color: #999;
}
.franchiseeCoo-cooperation-tip{
  padding-bottom: 145rpx;
  margin-top: 40rpx;
  font-size: 24rpx;
  color: #999;
  text-align: center;
}
.form-btn-wrap{
  position: fixed;
  bottom: -120rpx;
  left: 0;
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #F0F0F0;
  transition: bottom 200ms ease-in-out;
  -webkit-transition: bottom 200ms ease-in-out;
}
.form-btn-wrap.btn-fixed{
  bottom: 0rpx;
}
.franchiseeCoo-service{
  padding: 38rpx 0;
  text-align: center;
}
.franchiseeCoo-service-qrcode{
  width: 180rpx;
  height: 180rpx;
}
.franchiseeCoo-service-text{
  color: #666;
  font-size: 24rpx;
}
.sidebar{
  position: fixed;
  width: 82rpx;
  right: 0;
  bottom: 130rpx;
}
.sidebar > view{
  width: 82rpx;
  height: 82rpx;
  color: #fff;
  font-size: 44rpx;
  text-align: center;
  line-height: 82rpx;
  background-color: rgba(0, 0, 0, .4);
  border-radius: 4rpx;
  margin-top: 1px;
}