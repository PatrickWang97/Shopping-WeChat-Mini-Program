page{
  background-color: #f5f5f5;
}
.cs-search{
  background-color: #fff;
  padding: 20rpx 0 0;
}
.cs-search-bg{
  width: 710rpx;
  height: 68rpx;
  margin: 0 20rpx;
  overflow: hidden;
  color: #999;
  font-size: 28rpx;
  line-height: 68rpx;
  background-color: #f5f5f5;
  border-radius: 34rpx;
}
.cs-search-icon{
  font-size: 30rpx;
  color: #999;
  margin-right: 10rpx;
  margin-left: 20rpx;
  vertical-align: top;
}
.cs-search-focus{
  margin-left: 20rpx;
  font-size: 0;
}
.cs-search-input-wrap{
  display: inline-block;
  width: 630rpx;
  height: 68rpx;
  overflow: hidden;
  color: #333;
  font-size: 28rpx;
  line-height: 68rpx;
  background-color: #f5f5f5;
  border-radius: 34rpx;
}
.cs-search-input{
  display: inline-block;
  width: 500rpx;
  height: 68rpx;
  vertical-align: top;
}
.cs-search-input-placeholder{
  color:#999;
  font-size: 28rpx;
}
.cs-search-delete{
  display: inline-block;
  width: 60rpx;
  color: #cecece;
  font-size: 30rpx;
  text-align: center;
}
.cs-search-cancel{
  display: inline-block;
  width: 90rpx;
  color: #59607b;
  font-size: 28rpx;
  line-height: 68rpx;
  text-align: center;
  vertical-align: top;
}
.cs-head{
  font-size: 26rpx;
  color: #A6A6A6;
  line-height: 66rpx;
  padding: 0 30rpx;
  border-bottom: 1px solid #EEEEEE;
}
.cs-lately,
.cs-nearby{
  background-color: #fff;
}
.cs-item{
  padding: 30rpx;
  border-bottom: 1px solid #EEEEEE;
}
.cs-titile{
  display: inline-block;
  max-width: 420rpx;
  font-size: 34rpx;
  color: #131313;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cs-business{
  display: inline-block;
  width: 94rpx;
  height: 37rpx;
  margin-left: 20rpx;
  margin-top: 6rpx;
  color: #999;
  font-size: 22rpx;
  line-height: 37rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: #F2F2F2;
  vertical-align: top;
}
.cs-visit-time{
  font-size: 30rpx;
  color: #A6A6A6;
  margin-top: 15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cs-lately{
  margin-top: 20rpx;
}
.cs-nearby{
  margin-top: 10rpx;
}
.cs-distance{
  float: right;
  width: 155rpx;
  margin-top: 6rpx;
  font-size: 30rpx;
  color: #A6A6A6;
  text-align: right;
  vertical-align: top;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cs-result{
  border-top: 1px solid #EEEEEE;
}
.cs-none-item .cs-image{
  background-color: #D2D2D2;
}
.cs-none-item .cs-titile{
  margin-top: 28rpx;
}
.cs-search-none{
  font-size: 26rpx;
  color: #A8A8A8;
  text-align: center;
  margin-top: 35rpx;
}
.cs-tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 96rpx;
  font-size: 26rpx;
  line-height: 96rpx;
  background-color: #fff;
}
.cs-tab .cs-tab-item {
  color: #666;
  flex-grow: 1;
  text-align: center;
}
.cs-tab .cs-tab-item.selected {
  position: relative;
  color: #fc811a;
}
.cs-tab .cs-tab-item.selected::after {
  content: '';
  position: absolute;
  bottom: 16rpx;
  left: 50%;
  width: 60rpx;
  height: 4rpx;
  border-radius: 2rpx;
  background-color: #fc811a;
  transform: translateX(-50%);
}
.cs-result-list1,
.cs-result-list2 {
  margin-top: 22rpx;
  background-color: #fff;
}
.cs-result-list1 .cs-item {
  display: flex;
  padding: 30rpx 30rpx 30rpx 20rpx;
  border-bottom: 1px solid #f5f5f5;
}
.cs-result-list1 .cs-item-img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 6rpx;
  background-color: #f5f5f5;
}
.cs-result-list1 .cs-item-txt {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 18rpx;
  color: #999;
}
.cs-result-list1 .cs-item-des {
  width: 522rpx;
}
.cs-result-list1 .cs-item-goods {
  margin-top: 14rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  color: #333;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.cs-result-list1 .cs-item-data {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 522rpx;
}
.cs-result-list1 .cs-item-price {
  display: flex;
  align-items: center;
}
.cs-result-list1 .cs-item-price1 {
  font-size: 32rpx;
  font-weight: bold;
  color: #fc811a;
}
.cs-result-list1 .cs-item-price1 text {
  font-size: 22rpx;
}
.cs-result-list1 .cs-item-price2 {
  margin-left: 20rpx;
  font-size: 28rpx;
  text-decoration:line-through;
}
.cs-result-list1 .cs-item-sales {
  font-size: 28rpx;
}
.cs-result-list2 .cs-item {
  display: flex;
  padding: 0 20rpx 22rpx;
  border-bottom: 10rpx solid #f5f5f5;
}
.cs-result-list2 .cs-item-logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 6rpx;
  margin-top: 12rpx;
  margin-right: 20rpx;
  background-color: #f5f5f5;
}
.cs-result-list2 .cs-item-detail {
  display: flex;
  flex-direction: column;
  width: 586rpx;
}
.cs-result-list2 .cs-item-desc {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  padding: 22rpx 0 20rpx;
  border-bottom: 1px solid #f5f5f5;
}
.cs-result-list2 .cs-item-shop {
  max-width: 440rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  color: #333;
}
.cs-result-list2 .cs-item-other {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666;
  margin-top: 20rpx;
}
.cs-result-list2 .cs-item-other text:first-child {
  position: relative;
  padding-right: 32rpx;
}
.cs-result-list2 .cs-item-other text:first-child::after {
  content: '';
  position: absolute;
  right: 16rpx;
  top: 54%;
  height: 20rpx;
  width: 2rpx;
  background-color: #999;
  transform: translateY(-50%);
}
.cs-result-list2 .cs-item-btn {
  height: 48rpx;
  padding: 0 28rpx;
  border: 1px solid #fc811a;
  border-radius: 6rpx;
  font-size: 22rpx;
  line-height: 48rpx;
  text-align: center;
  color: #fc811a;
}
.cs-result-list2 .cs-item-goods {
  display: flex;
  margin: 18rpx 0;
}
.cs-result-list2 .goods-item {
  position: relative;
  width: 190rpx;
  height: 163rpx;
  margin-right: 10rpx;
  overflow: hidden;
}
.cs-result-list2 .goods-item:last-child {
  margin-right: 0;
}
.cs-result-list2 .goods-img {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  background-color: #f5f5f5;
}
.cs-result-list2 .goods-price {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  width: 190rpx;
  height: 66rpx;
  border-radius: 10rpx;
  font-size: 22rpx;
  line-height: 76rpx;
  text-align: center;
  color: #fff;
  background:linear-gradient(0deg,rgba(0,0,0,.5) 0%,rgba(0,0,0,0) 100%);
}
.cs-result-list2 .cs-item-coupons {
  display: flex;
  flex-direction: column;
  margin-top: 18rpx;
  font-size: 24rpx;
  color: #333;
}
.cs-result-list2 .coupons-item {
  display: flex;
  align-items: center;
}
.cs-result-list2 .coupons-mark {
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  line-height: 26rpx;
  text-align: center;
  color: #fff;
  background-color: #fc811a;
}
.cs-result-list2 .coupons-name {
  max-width: 550rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.cs-result-list3 {
  padding: 0 25rpx;
}
.cs-result-list-null {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 166rpx;
}
.cs-result-list-null image {
  width: 173rpx;
  height: 120rpx;
  margin-left: 26rpx;
}
.cs-result-list-null text {
  margin-top: 32rpx;
  font-size: 26rpx;
  color: #999;
}