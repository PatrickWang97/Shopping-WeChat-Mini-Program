page {
  height: 100%;
  background-color: #F3F3F3;
  font-size: 29rpx;
}
.myOrder-tab-list {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  line-height: 80rpx;
  height: 80rpx;
  font-size: 26rpx;
  background: #fff;
  text-align: center;
}
.myOrder-tab-list view {
  border-bottom: 2px solid transparent;
}
.myOrder-tab-list view.active {
  border-color: #ff7100;
  color: #ff7100;
}
.myOrder-content-body {
  height: calc(100% - 154rpx);
  margin-top: 169rpx;
}
.scrollContainer {
  height: 100%;
}
.myOrder-list-item {
  background-color: #fff;
  margin: 0 auto;
  margin-bottom: 16rpx;
  width: 702rpx;
  border-radius: 16rpx;
}
.myOrder-list-title {
  height: 60rpx;
  margin-bottom: 20rpx;
  margin: 0 30rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid rgb(243, 243, 243);
}
.myOrder-list-title .title-time {
  display: block;
  font-size: 24rpx;
  color: rgb(153, 153, 153);
}
.myOrder-list-title .title-time text {
  float: right;
  color: #ff7100;
}
.myOrder-list-goods {
  margin-bottom: 7rpx;
}
.goods-item {
  height: 160rpx;
  padding: 14rpx 30rpx 0;
  margin-bottom: 7rpx;
}
.goods-img {
  float: left;
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
  margin-right: 42rpx;
  background-color: #fff;
}
.goods-img image {
  display: block;
  width: 100%;
  height: 100%;
}
.goods-describe {
  float: left;
  width: calc(100% - 182rpx);
  height: 150rpx;
  position: relative;
}
.goods-describe .goods-describe-title, .goods-describe .goods-describe-price,
.goods-describe .goods-describe-model {
  display: block;
  color: #333333;
  font-size: 26rpx;
}
.goods-describe .goods-describe-model {
  font-size: 24rpx;
  color: #999999;
  margin: 10rpx 0 15rpx;
}
.goods-describe .goods-describe-price {
  font-size: 30rpx;
  color: #333333;
  position: absolute;
  bottom: 0;
  width: 100%;
  line-height: 40rpx;
}
.goods-describe .goods-describe-price .refunded {
  position: absolute;
  right: 0;
  bottom: 0;
  text-align: right;
}
.goods-describe .goods-describe-price .refunded view {
  font-size: 24rpx;
  color: #666666;
}
.goods-describe .goods-describe-price .refunded  .refunded_num{
  color: #FF7100;
}
.goods-describe .goods-describe-price text {
  float: right;
  color: #666;
  font-size: 24rpx;
}
.list-footer-btn {
  height: 80rpx;
  line-height: 80rpx;
  text-align: right;
  padding: 0 30rpx;
}
.list-footer-label {
  padding: 20rpx 30rpx;
  color: #666666;
  text-align: right;
  border-bottom: 1rpx solid rgb(243, 243, 243);
  font-size: 24rpx;
}
.list-footer-label .origin {
  color: #ff7100;
}
.list-footer-btn text {
  display: inline-block;
  width: 174rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 10rpx;
  border-radius: 25rpx;
  border: 1rpx solid rgba(255, 113, 0, 1);
  text-align: center;
  color: rgba(255, 113, 0, 1);
  font-size: 24rpx;
}
.activity {
  position: fixed;
  top: 80rpx;
  left: 0;
  width: calc(100% - 60rpx);
  height: 76rpx;
  line-height: 76rpx;
  padding: 0 30rpx;
  background: #FFFAEE;
  color: #ff7100;
  font-size: 24rpx;
}
.activity > text:nth-child(1) {
  display:inline-block;
  max-width: 600rpx;
}
.noMoreNewDataTip {
  text-align: center;
  color: #aaa;
  line-height: 30px;
  margin-bottom: 10px;
}  
.tostore-list-submenu view.active {
  color: #ff7100;
  font-weight:bold;
}
.tostore-sub-box {
  display: inline-block;
  padding: 0 16rpx;
  margin: 0 10rpx;
  position: relative;
}
.tostore-list-submenu {
  overflow-x: auto;
  white-space: nowrap;
  background: #fff;
  color: #666;
  border-bottom: none;
}
.delivery-type-icon {
  float: left;
  font-size: 22rpx;
  color: #ff7100;
  width: 96rpx;
  line-height: 34rpx;
  height: 34rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-top: 14rpx;
  background: #FFFAEE;
  padding: 2rpx;
}
.tostore-sub-box .bottom-line {
  position: absolute;
  display: inline-block;
  width: 40rpx;
  height: 4rpx;
  background: #FF7100;
  bottom: 12rpx;
  border-radius: 4rpx;
  left: 50%;
  transform: translateX(-50%);
  visibility: hidden;
}
.tostore-list-submenu view.active .bottom-line {
  visibility: initial;
}
.take {
  height: 98rpx;
  padding: 0 30rpx;
  display: flex;
  font-size: 26rpx;
  align-items: center;
}
.take-tips {
  width: 130rpx;
  line-height: 98rpx;
  color: #666666;
}
.take-info {
  width: 456rpx;
}
.take-icon {
  width: 56rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border-radius: 50%;
  background: #FFF0E5;
  color: #FF7100;
  font-size: 34rpx;
}
.take-info .take-phone {
  margin-left: 14rpx;
  color: #FF7100;
}
.list-footer-buyerInfo {
  padding: 0 30rpx;
  font-size: 26rpx;
  position: relative;
}
.list-footer-buyerInfo .buyerInfo {
  height: 98rpx;
  line-height: 98rpx;
}
.list-footer-buyerInfo .detailAddress {
  margin-top: -16rpx;
  padding-bottom: 20rpx;
  padding-left: 140rpx;
  padding-right: 80rpx;
}
.icon-takeout-cellphone-left {
  height: 56rpx;
  width: 56rpx;
  border-radius: 50%;
  color: #FF7100;
  font-size: 36rpx;
  text-align: center;
  line-height: 56rpx;
  background: #FFF0E5;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}