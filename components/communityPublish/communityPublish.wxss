@charset 'utf-8';
@import "/utils/font_icon.wxss";
.communityPublish-wrap{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #fff;
  z-index: 51;
  overflow-y: auto;
}
.communityPublish-main{
  padding: 15px;
  background-color: #fff;
}
.communityPublish-title {
  height: 48px;
}
.communityPublish-title-input{
  width: 100%;
  height: 48px;
  border: none;
  border-bottom: 1px solid #E7E7E7;
  outline: none;
  font-size: 36rpx;
  color: #333;
}
.communityPublish-content {
  height:150px;
}
.communityPublish-content-input{
  width: 100%;
  height: 150px;
  padding-top: 16rpx;
  border: none;
  resize: none;
  outline: none;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #666;
  overflow: hidden;
  box-sizing: border-box;
}
.communityPublish-content .communityPublish-content-view {
  width: 100%;
  height: 100px;
  padding-top: 10px;
  font-size: 28rpx;
  line-height: 30rpx;
  color: #343434;
  overflow-y: auto;
  white-space: pre-wrap;
  word-break: break-all;
}
.communityPublish-content-input.hide,
.communityPublish-title-input.hide,
.communityPublish-address-input.hide {
  display: none;
}
.communityPublish-img-wrap{
  margin-top: 5px;
}
.communityPublish-ul {
  display: flex;
  flex-wrap: wrap;
}
.communityPublish-item, .communityPublish-item-add{
  width: 220rpx;
  height: 220rpx;
  position: relative;
  margin: 10rpx 13rpx 10rpx 0;
  border-radius: 10px;
  overflow: hidden;
}
.communityPublish-item-add.communityPublish-uploadImg,
.communityPublish-img-wrap .communityPublish-item:nth-child(3n+3){
  margin-right: 0;
}
.communityPublish-item .communityPublish-upload-image{
  width: 100%;
  height: 100%;
  border-radius: 10px;
}
.communityPublish-item-delete{
  display: block;
  width: 16px;
  height: 17px;
  text-align: center;
  line-height: 16px;
  font-size: 16px;
  color: #fff;
  background-color: rgba(0,0,0,.26);
  position: absolute;
  top: -1px;
  right: -1px;
}
.communityPublish-item-add{
  text-align: center;
  color: #cbcbcb;
  border: 1px dashed #979797;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  overflow: hidden;
  margin-bottom: 10px;
}
.communityPublish-item-add.tip {
  display: flex;
  width: auto;
  height: auto;
  align-items: flex-end;
  text-align: left;
  font-size: 24rpx;
  color: #999;
  border: 0;
  margin-top: 0;
}
.communityPublish-item-add .add-label {
  font-size: 35px;
  margin-top: 50rpx;
}
.communityPublish-tip{
  color: #a3a3a3;
  margin-top: 15px;
  font-size: 0.6em;
}
.communityPublish-btn{
  position: fixed;
  left: 30rpx;
  bottom: 20rpx;
  display: block;
  width: 690rpx;
  height:44px;
  line-height:44px;
  border: none;
  color: #fff;
  background-color: #3091F2;
  margin: 0 auto;
  border-radius: 3px;
  z-index: 99;
}
.separate-line{
  background: #eee; 
  height: 20rpx; 
  width: 100%;
}
.communityPublish-center {
  padding: 0 15px;
  background: #fff;
  color: #6e6e6e;
}
.communityPublish-center .communityPublish-piece {
  display: flex;
  align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 14px;
  border-bottom: 1px solid #E7E7E7;
}
.communityPublish-center .communityPublish-piece-icon {
  margin-right: 8px;
  font-size: 16px;
  color: #d8d8d8;
}
.communityPublish-classify .icon-rightarrow {
  font-size: 14px;
}
.communityPublish-center .communityPublish-phone-input {
  flex: 1;
  color: #343434;
}
.communityPublish-phone {
  border-bottom: 1px solid #E7E7E7;
}
.communityPublish-address {
  display:flex;
  align-items:center;
  margin-top: 10px;
  height: 90rpx;
  line-height: 90rpx;
  font-size:12px;
  border-bottom: 1px solid #E7E7E7;
}
.communityPublish-address .communityPublish-address-input {
  flex:1;
}
.communityPublish-address .communityPublish-address-label {
  margin-right:8px;
  font-size:16px;
  color:#d8d8d8;
}
.communityPublish-center .communityPublish-piece-icon.colorBlur,
.communityPublish-address .communityPublish-address-label.colorBlur {
  color: #3091f2;
}
.communityPublish-classify .communityPublish-classify-view{
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  color: #6e6e6e;
}
.communityPublish-classify .communityPublish-classify-view .communityPublish-classify-text {
  display: inline-block;
  vertical-align: top;
  font-size: 28rpx;
  margin:0 10rpx;
  max-width: 200rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.communityPublish-classify .communityPublish-classify-view .communityPublish-piece-icon {
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
}
.communityPublish-current-classify {
  font-size: 30rpx;
  color: #333;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.communityPublish-classify-select-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.3);
  z-index: 99999;
}
.communityPublish-catchtouchmove-layer {
  width: 100%;
  height: 100%;
}
.communityPublish-classify-select-window {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 684rpx;
  background: #fff;
  border-radius: 10rpx 10rpx 0 0;
}
.communityPublish-classify-select-top {
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 90rpx;
  color: #379BFF;
}
.communityPublish-classify-select-top .communityPublish-cst-text {
  position: relative;
  display: inline-block;
  margin-right: 20rpx;
  max-width: 200rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.communityPublish-classify-select-top .communityPublish-cst-text.active::after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 2px;
  background: linear-gradient(270deg,rgba(129,204,255,1) 0%,rgba(55,155,255,1) 100%);
}
.communityPublish-classify-select-list {
  height: 560rpx;
  overflow-x: hidden;
  overflow-y: auto;
}
.communityPublish-classify-select-item {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 30rpx;
  height: 102rpx;
  border-bottom: 1px solid #E5E5E5;
  color: #343434;
}
.communityPublish-classify-select-item .communityPublish-csi-image {
  display: inline-block;
  vertical-align: middle;
  width: 78rpx;
  height: 78rpx;
  background: #f8f8f8;
}
.communityPublish-classify-select-item .communityPublish-csi-text {
  flex: 1;
  overflow: hidden;
  display: inline-block;
  vertical-align: middle;
  line-height: 102rpx;
  margin-left: 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #333;
}
.communityPublish-classify-select-item .communityPublish-csi-icon {
  display: inline-block;
  vertical-align: middle;
  font-size: 26rpx;
  color: #999;
}
.return-btn {
  position: fixed;
  bottom: 155rpx;
  right: 32rpx;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  background: #fff;
  border-radius: 50%;
  font-size: 35rpx;
  color: #888;
  box-shadow: 0 0 18rpx rgba(0,0,0,.13);
}
.return-btn label{
  display: block;
  color: #666;
}
.return-btn label:nth-child(1){
  font-size: 14px;
  margin-top: -7px;
  height: 28rpx;
}
.return-btn .return-text{
  font-size: 9px;
}
.communityPublish-setTop {
  box-sizing: border-box;
  width: 100%;
  height: 160px;
  padding: 10rpx 30rpx;
  background-color: #fff;
  color: #333;
}
.flex-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.communityPublish-seting-top {
  height: 90rpx;
  border-top: 4rpx solid #d7d7d7;
}
.communityPublish-seting-top .switch {
  zoom: 0.68;
}
.communityPublish-seting-time {
  padding-left: 10rpx;
  height: 60rpx;
}
.communityPublish-seting-money {
  height: 60rpx;
  padding-left: 10rpx;
  padding-right: 30rpx;
}
.communityPublish-seting-time .icon-rightarrow {
  padding-left: 10rpx;
  color: #979797;
  font-weight: 600;
}
.communityPublish-classify-title{
  width: 80%;
  float: left;
}
.communityPublish-classify{
  width: 20%;
}
.communityPublish-uploadImg{
  background:#F9F9F8 ;
}
.upload-text{
  font-size: 22rpx;
  color: #666;
  display: block;
}
.communityPublish-center input{
  color: #333;
  font-size: 30rpx;
}
.communityPublish-foot{
  height: 160rpx;
}
.communityPublish-uploadImg .icon-upload-picture{
  color: #333;
}
.communityPublish-center .icon-fesadress,
.communityPublish-center .icon-topic-classify,
.communityPublish-center .icon-telephone{
  color: #666;
  font-size: 18px;
}