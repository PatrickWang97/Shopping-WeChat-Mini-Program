.page-qr-code-dialog {
  position: fixed;
  left: 0;
  right: 0;
  top: 100%;
  bottom: 0;
  z-index: 50;
  background-color: rgba(0, 0, 0, .4);
}
.page-qr-code {
  position: absolute;
  left: 50%;
  top: 50%;
  border-radius: 10rpx;
  text-align: center;
  background: #fff;
  overflow: auto;
  padding-bottom: 30rpx;
  transform: translate(-50%, -50%);
}
.page-bottom-dialog {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 750rpx;
  background-color: rgba(238, 238, 238, 1);
}
.button {
  border-radius: 0;
  height: 100rpx;
  color: #666;
  font-size: 28rpx;
  line-height: 100rpx;
}
.shareToMoments {
  border-top: 1px solid #f4f4f4;
}
.cancelShare{
  margin-top: 20rpx;
}
.goods-share-canvas {
  height: 410px;
  width: 280px;
}
.group-canvas{
  width: 280px;
  height: 435px;
}
.save-btn {
  width: 300rpx;
  display: inline-block;
  margin: 0 auto;
  color: #fff;
  font-size: 28rpx;
  line-height: 70rpx;
  border-radius: 4rpx;
  text-align: center;
  background: #FF3600;
}
.canvas2d {
  margin-bottom: 10px;
}