.mainpage {
  align-items: center;
}
.countText {
  position: absolute;
  width: 28px;
  height: 18px;
  min-height: 18px;
  font-size: 16px;
  margin-left:2px;
  margin-top:5px;
  text-align: center;
  color:rgba(248,89,89,1);
}
.back {
  width: 100%;
  position: absolute;
  z-index: -100;
  height: 100%;
}
.bigTitle {
  margin-top: 20px;
  width: 100%;
  height: 180rpx;
}
.topSlider {
  display: block;
  margin: 12px auto 0;
  height: 40px;
  width: 660rpx;
}
.sliderLineBack {
  background-color: #8b8ec3;
  height: 4px;
  border-radius: 6px;
  width: 98%;
  margin-left:1%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  overflow: hidden;
}
.lightLine {
  display: block;
  border-radius: 6px;
  height: 100%;
  background-color: #f8e71c;
}
.slider {
  position: absolute;
  height: 72px;
  width: 72px;
  top:-12rpx;
}
.topText {
  margin-top: 0px;
  height: 20px;
  font-size: 16px;
  text-align: center;
  color: #f8e71c;
}
.backPad {
  margin-top: 8px;
  min-height: 200rpx;
  margin-left: 20px;
  margin-right: 20px;
  display: flex;
  flex-wrap: wrap;
}
.cardPad {
  width: 25%;
  display: block;
  text-align: center;
  position: relative;
}
.cardItem {
  display: inline-block;
}
.cardBackground {
  position: absolute;
  display: block;
  left: 0px;
  right: 0px;
  top: 0px;
  bottom: 0px;
  box-sizing: border-box;
  background: rgba(255,255,255,0.2);  
  border-radius: 6px;
  margin: 2px;
  z-index:-1; 
}
.tagNumber {
  position: absolute;
  height: 68rpx;
  width: 60rpx;
  padding-left: 84rpx;
  padding-top: 4rpx;
  text-align: center;
  font-size: 14px;
  color: #fff;
}
.cardTag {
  position: absolute;
  height: 54rpx;
  width: 44rpx;
  padding-left: 94rpx;
  padding-top: 4rpx;
}
.cardImage {
  margin-top: 6px;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 0px;
  width: 144rpx;
  height: 144rpx;
  background-color: #9b9b9b;
  border-radius: 4px;
  opacity: 1;
}
.nameText {
  font-size: 14px;
  color: white;
  text-align: center;
  width: 100%;
  padding-bottom: 2px;
  margin-top: -5px;
}
.submitButton {
  margin-top: 16px;
  width: 100%;
  height: 120rpx;
}
.backGroundView {
  width: 100%;
  display: block;
}
.back {
  width: 100%;
  position: fixed;
  z-index: -100;
  height: 100%;
}
page {
  height: 100%;
}
.bottomTitle {
  opacity: 0.5;
  height: 24px;
  width: 100px;
  margin-left: auto;
  margin-right: auto;
  display: block;
  text-align: center;
  border-radius: 4px;
  background-color: white;
  line-height: 24px;
  font-size: 14px;
  color: black;
}
.intro_word{
  position: fixed;
  bottom: 10rpx;
  left: 8rpx;
  font-size: 22rpx;
  color: #FFFFFF;
}
.outside{
height:10px; 
background:rgba(17,25,110,1);
opacity:0.5;
border-radius: 7rpx;
position: absolute;
top: 50%;
transform: translateY(-50%); 
width: 100%;
}
.login{
  position: fixed;
  width: 550rpx;
  height: 270rpx;
  background: white;
  left:100rpx;
  top:400rpx;
  border-radius: 14rpx;
  z-index: 1000;
}
.login_text{
  width: 350rpx;
  height: 130rpx;
  line-height: 130rpx;
  margin-left:100rpx;
  text-align: center;
}
.login_btn{
  width: 350rpx;
  height: 100rpx;
  margin-left:100rpx;
}
.version{
  position: fixed;
  top:0;
  right: 0;
  height: 20rpx;
  width: 20rpx;
}
